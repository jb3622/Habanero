	//Reset Form Controls and clear Grid
	BegSr btnResetFilters_Click Access(*Private) Event(*this.btnResetFilters.Click)
		DclSrParm sender *Object
		DclSrParm e System.EventArgs
		
		DclFld arrIndex	Type(Int16)

		txtVendorName.Text	    = String.Empty
		VendorDV                = *Nothing
		dvVendor				= *Nothing
		VendorString			= *Nothing
		RDOSpice.Checked        = *True
		RDOIP.Checked           = *False
		txtFromPOrange.Text     = String.Empty
		PORangeFrom             = *Nothing 
		txtToPORange.Text       = String.Empty
		PORangeTo               = *Nothing
		chkShowMyPO.Checked     = *True
		btnApplyFilters.Enabled = *True
				
		//Market/Store
		cmbMarkets.ResetText()
		cmbMarkets.SelectedText = 'All Markets'
		cmbStores.ResetText()
		cmbStores.DataSource    = *Nothing
		cmbStores.SelectedText  = 'All Stores'
		Market_Select           = *Nothing
		Store_Select            = *Nothing
						
		//StageSetDate
		If StageSetDate <> *Nothing
			cmbSSD.ResetText()
			cmbSSD.SelectedIndex = 0
			StageSetdate = *Nothing
		Endif
		
		//Family/Character
        Char_Select  = *Nothing
        cmbFamily.ResetText()
		cmbFamily.SelectedText = 'All Families'
		ItemsBO.GetAllFamilyCharTbl("All Families")
			
		cmbFamily.DataSource = SpiceDS.Tables("AllFamily")
		cmbFamily.DisplayMember = "FamilyCodeDescription"
		cmbFamily.ValueMember   = "FamilyCode"
		AddHandler SourceObject(cmbFamily) SourceEvent(SelectedValueChanged) HandlerSR(cmbFamily_SelectedValueChanged)		
		
		chkLstCharacter.DataSource = SpiceDS.Tables("FamilyChars")
		chkLstCharacter.DisplayMember = "CharCodeDescription"
		chkLstCharacter.ValueMember   = "CharCode"
		chkLstCharacter.ClearSelected()
		
		For Index(arrIndex = 0) to(chkLstCharacter.Items.Count -1)
			chkLstCharacter.SetItemChecked(arrIndex, *False)
		EndFor
		
		Char_Select = *Nothing

		//Class
		chkLstClass.ClearSelected()
		For Index(arrIndex = 0) to(chkLstClass.Items.Count -1)
			chkLstClass.SetItemChecked(arrIndex, *False)
		EndFor
		
		Class_Select = *Nothing
		
		//PO Status
		chkLstPOStatus.ClearSelected()
		For Index(arrIndex = 0) to(chkLstPOStatus.Items.Count -1)
			chkLstPOStatus.SetItemChecked(arrIndex, *False)						
		EndFor
		
		POStatus_Select = *Nothing
		
		//Ship Dates
		If ShipDateFrom <> *Nothing and ShipDateTo <> *Nothing 
		   dtpFromShipDate.ResetText()
		   dtpToShipDate.ResetText()
		   ShipDateFrom = *Nothing
		   ShipDateTo   = *Nothing
		Endif
		
		//Order Dates
		If OrderDateFrom <> *Nothing and OrderDateTo <> *Nothing 
		   dtpFromOrderDate.ResetText()
		   dtpToOrderDate.ResetText()
		   OrderDateFrom = *Nothing
		   OrderDateTo   = *Nothing
		Endif
				
		DoWhile dgvPOMain.RowCount > 0
			dgvPOMain.Rows.RemoveAt(0)
		Enddo

		dtOrderH = *Nothing
	EndSr
	
