	//Vendor build DataView from entered text......Builds a DV of Vendor numbers, Vendor Names and Depts based on user departments
	BegSr txtVendorName_Leave Access(*Private) Event(*this.txtVendorName.Leave)
		DclSrParm sender *Object
		DclSrParm e System.EventArgs
		
		ErrorMsg.SetError(txtVendorName, string.empty)
		btnApplyFilters.Enabled = *True
				
		If txtVendorName.Text.Length >= 3 or txtVendorName.Text = string.Empty
		
			//Build Vendor department specific DataView
			If txtVendorName.Text <> string.Empty
				SpiceDS = ItemsBO.GetVendorTbl()
				
				dvVendor = *New DataView(SpiceDS.Tables["FilteredVendors"])
				VendorString = *New StringBuilder()
				VendorString.Append("VendorName ") 
				VendorString.Append(" Like '")
				VendorString.Append(txtVendorname.Text.ToString())
				VendorString.Append("%'")
				
				dvVendor.RowFilter = VendorString.ToString()
				
				//copy DataView for send 
				VendorDV = dvVendor
			Else
				VendorDV = *Nothing	
			Endif
		Else
			ErrorMsg.SetError(txtVendorName, "Vendor Name Search must contain at least 3 Characters")
			btnApplyFilters.Enabled = *False
			LeaveSR	
		Endif	
	EndSr
		
