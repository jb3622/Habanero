	BegSr PadlockSR
		
		DclFld Count      Type(Int32)
		DclFld NewStatus  Type(String)
		
		//Set Padlock details on the grid
		PurchaseOrder.PadlockSR()
						
		For Index(Count = 0) To(dgvPOMain.RowCount -1)
			If dgvPOmain['clmPadlock', Count].Value *As *String = '0'
				dgvPOMain['clmChangeID', Count].Value = ImageList.Images(0)
				dgvPOMain['clmSelect', Count].Value = *False
				dgvPOMain['clmSelect', Count].ReadOnly = *True 
				NewStatus = PurchaseOrder.GetPOStatus(dgvPOMain['clmSpicePO', Count].Value *As *String)
				dgvPOMain['clmStatus', Count].Value = NewStatus
			EndIf
		EndFor
	EndSr
	
	BegFunc AddZeros Type(*Char) Len(5) Access(*Private)
		DclSrparm HoldChar Type(*Char) Len(5)
		
		DclFld Do_Index   Type(Int16)
		DclFld Blank      Type(*Char) Len(1)
				
		HoldChar2 = HoldChar
		Do 1 5 Do_Index
			Blank = %SubST(HoldChar2, Do_Index, 1)
			If Blank = ' '
				HoldChar2 = %Replace('0', HoldChar2, Do_Index, 1)
			Endif
		Enddo
		
		LeaveSr HoldChar2
	EndFunc
