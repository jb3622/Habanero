	//Validate PO Range
	BegSr txtToPORange_Leave Access(*Private) Event(*this.txtToPORange.Leave)
		DclSrParm sender *Object
		DclSrParm e System.EventArgs
		
		DclFld Length     Type(*Packed) Len(5,0) Inz(0)
				
		ErrorMsg.SetError(txtFromPOrange, string.empty)
		btnApplyFilters.Enabled = *True
		
		POFrom = String.Empty
		POTo   = String.Empty
		
		If txtFromPORange.Text <> "" and txtToPORange.Text <> ""
			POFrom = txtFromPORange.Text.ToString()     
			POTo = txtToPORange.Text.ToString()
				
			Length = POFrom.Length
			If Length <> 5
				Move POFrom HoldChar
				AddZeros(HoldChar)
				POFrom = HoldChar2
			Endif
				
			Length = POTo.Length
			If Length <> 5
				Move POTo HoldChar
				AddZeros(HoldChar)
				POTo = HoldChar2
			Endif
											
			If POFrom > POTo
				ErrorMsg.SetError(txtFromPOrange, "PO Range not valid")
				btnApplyFilters.Enabled = *False
				LeaveSr
			Endif   	
		Endif
		
		//Vars sent to DSSPOHCLS for Order filters
		If POFrom <> "" and POTo <> ""
			PORangeFrom = POFrom
			PORangeTo   = POTo
		Else
			POrangeFrom = String.Empty
			POrangeTo   = String.Empty
		Endif
	EndSr
	
