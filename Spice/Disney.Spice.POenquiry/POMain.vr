	BegSr POmain_Load Access(*Private) Event(*this.Load)
		DclSrParm sender *Object
		DclSrParm e System.EventArgs
       	
       	DclFld ImagePath Type(String)
       	
		//Create Image List
        ImagePath = System.IO.Path.GetDirectoryName(System.Reflection.Assembly.GetExecutingAssembly().CodeBase).Substring(6)
        try
			ImageList.Images.Add(Image.FromFile(Path.Combine(ImagePath, "PadLock.png")))
        catch Ex Type(System.Exception)
            MsgBox "Traffic light images are missing"
        EndTry
        					
		//Set controls
		btnSubmitToEas.Enabled = *False
		btnPrintEDI.Enabled    = *False
		btnCancelPO.Enabled    = *False
		btnChangeSSD.Enabled   = *False
		cmbStores.SelectedText = 'All Stores'
		chkShowMyPO.Checked    = *True
		rdoSpice.Checked       = *True
		RdoChoice              = 'Spice'
		CheckBoxCount          = 0
		
		//Stage set Date not used in Europe		
		//If Environments.Domain = 'EMEA'
		If (DataCache.AreStageSetDatesChangeable = *False)
			cmbSSD.Enabled       = *False
			btnChangeSSD.Enabled = *False
		Endif
		
		//Set DatePicker Date Format
		If Environments.DateFormat = "DMY"
			dtpFromShipDate.CustomFormat  = "dd MMMM yyyy"
			dtpToShipDate.CustomFormat    = "dd MMMM yyyy"
			dtpFromOrderDate.CustomFormat = "dd MMMM yyyy"
			dtpToOrderDate.CustomFormat   = "dd MMMM yyyy"
		Else
			dtpFromShipDate.CustomFormat  = "MMMM dd yyyy"
			dtpToShipDate.CustomFormat    = "MMMM dd yyyy"
			dtpFromOrderDate.CustomFormat = "MMMM dd yyyy"
			dtpToOrderDate.CustomFormat   = "MMMM dd yyyy"
		Endif
				
		// Call methods in ItemsBO to build dataset
		ItemsBO.GetAllFamilyTbl()
		ItemsBO.GetAllFamilyCharTbl("All Families")
		ItemsBO.GetClassTbl()
		ItemsBO.GetAuthorisedMarketsTbl()
		SpiceDS = ItemsBO.SpiceDataSet
		
		//Get Authorised Departments
		dvDepartments = *New DataView(SpiceDS.Tables["AuthorisedDepts"])
										
		//Stage Set dates only built when needed DropDownList
		dtStageSetDates                  = PurchaseOrder.StageSetDates()
		dtStageSetDates.DefaultView.Sort = "clmSSDDATNum DESC"	
		cmbSSD.DataSource                = dtStageSetDates
		
		If Environments.DateFormat = "DMY"
			cmbSSD.DisplayMember = "clmSSDDATDMY" 
		Else
			cmbSSD.DisplayMember = "clmSSDDATMDY" 
		Endif
		
		AddHandler SourceObject(cmbSSD) SourceEvent(SelectedValueChanged) HandlerSR(cmbSSD_SelectedValueChanged)	
		
		//Markets DropDownList
		dtMarkets = SpiceDS.Tables("AuthorisedMarkets").Copy()
		drMarkets = dtMarkets.NewRow()
		drMarkets.Item["IPMarket"]     = 'All Markets'
		drMarkets.Item["IPMarketDesc"] = ' '
		dtMarkets.Rows.Add( drMarkets )
		cmbMarkets.DataSource = dtMarkets
		cmbMarkets.DisplayMember = "IPMarket" 
		
		//Stores DropDownList
		dvStores = PurchaseOrder.StoresAll(Environments.Domain)
		Market_Select = cmbMarkets.Text.ToUpper().Trim().ToString()
		cmbStores.DataSource = dvStores
		cmbStores.DisplayMember = "StoreDescription"
		
		AddHandler SourceObject(cmbMarkets) SourceEvent(SelectedValueChanged) HandlerSR(cmbMarkets_SelectedValueChanged)
		
		//PO Status ChkListBox
		dtStatus = PurchaseOrder.POStatus()
		dvStatus = *New DataView(dtStatus)
		chkLstPOStatus.DataSource = dvStatus
		chkLstPOStatus.DisplayMember = "clmStatusDesc"
												
		// Family
		cmbFamily.DataSource = SpiceDS.Tables("AllFamily")
		cmbFamily.DisplayMember = "FamilyCodeDescription"
		cmbFamily.ValueMember   = "FamilyCode"
		AddHandler SourceObject(cmbFamily) SourceEvent(SelectedValueChanged) HandlerSR(cmbFamily_SelectedValueChanged)		
		
		// Character
		chkLstCharacter.DataSource = SpiceDS.Tables("FamilyChars")
		chkLstCharacter.DisplayMember = "CharCodeDescription"
		chkLstCharacter.ValueMember   = "CharCode"

		// Class
		chkLstClass.DataSource = SpiceDS.Tables("FilteredClasses")
		chkLstClass.DisplayMember = "ClassCodeName" 		
	EndSr
