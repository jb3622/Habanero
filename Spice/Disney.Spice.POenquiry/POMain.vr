		DclFld StatusOfPO Type(String)
		
		If e.ColumnIndex < 0 Or e.RowIndex < 0
		   LeaveSR
		Endif
		
		dgvr = dgvPOMain.Rows[e.RowIndex]				
		If dgvPOMain.Item['clmSelect', e.RowIndex].ReadOnly = *True 
			LeaveSr
		Endif
		
		dgvPOMain['clmStatus', e.RowIndex].Value  = PurchaseOrder.GetPOStatus(dgvPOMain['clmSpicePO', e.RowIndex].Value *As *String)
				
		StatusChk = PurchaseOrder.CheckStatus(dgvr.cells["clmSpicePO"].Value.ToString(), dgvr.cells["clmIPPO"].Value.ToString())
		If StatusChk = *True
			dgvPOMain.Item['clmChangeID', e.RowIndex].Value = ImageList.Images(0)
			LeaveSr
		Endif
		
		StatusOfPO = PurchaseOrder.GetPOStatus(dgvr.cells["clmSpicePO"].Value.ToString())
		If StatusOfPO = 'OA' Or StatusOfPO = 'RA'
			MsgBox  Msg('The status of this PO prevents it from being modified at this stage')
			LeaveSr
		Endif
						
		GridPO = dgvr.cells["clmSpicePO"].Value.ToString()
		
		SetMousePtr *HourGlass
			
		//Instance the Class
		POmodification = *New POmodification(PgmDB, User, Environments, GridPO)	
		POmodification.ShowDialog()
		
		POmodification = *Nothing	
	EndSr
	
	BegSr dgvPOMain_CellContentClick Access(*Private) Event(*this.dgvPOMain.CellContentClick)
		DclSrParm sender *Object
		DclSrParm e System.Windows.Forms.DataGridViewCellEventArgs
				
		If e.RowIndex < 0 Or e.ColumnIndex < 0
		   LeaveSR
		Endif
		
		//Only do the EndEdit if the user has clicked on the select cell and a valid row.
		If dgvPOMain.Columns[e.ColumnIndex].Name.Equals("clmSelect") and +
			dgvPOMain.Item["clmPadlock", e.RowIndex].value.ToString() = '1'
			dgvPOMain.EndEdit()
		Endif
		
		//User has clicked on the grid checkbox. Enable buttons		
		If dgvPOMain.Item["clmSelect", e.RowIndex].Value *As Boolean = *True
			btnSubmitToEas.Enabled = *True
			btnPrintEDI.Enabled    = *True
			btnCancelPO.Enabled    = *True  
			
			//Only activate the change stage set dates button if not Europe
			// If Environments.Domain <> 'EMEA'
			If (DataCache.AreStageSetDatesChangeable = *True)
				btnChangeSSD.Enabled   = *True
			Endif
			
			CheckBoxCount += 1
		Endif
		
		//If the user has unchecked the checkbox decrement the count
		If dgvPOMain.Item["clmSelect", e.RowINdex].Value *As Boolean = *False and CheckBoxCount > 0
			CheckBoxCount -= 1
		Endif
		
		//If the count is back down to 0 switch off the buttons
		If CheckBoxCount           = 0
			btnSubmitToEas.Enabled = *False
			btnPrintEDI.Enabled    = *False
			btnCancelPO.Enabled    = *False
			btnChangeSSD.Enabled   = *False
		Endif
	EndSr
	
