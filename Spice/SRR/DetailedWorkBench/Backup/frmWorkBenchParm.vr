	// Apply any changes before any drilling
	BegFunc CheckChanges  Type(*Boolean)  Access(*Public)
	
		DclFld Action      Type(*Packed) Len(1,0)
				
		// No Changes
		If uxApply.Enabled = *False
			LeaveSR *True
		Endif
		
		// If changes then prompt user 
		If uxApply.Enabled = *True
			MsgBox 'You are about to lose your changes. Would you like to Apply your changes: ' RtnCode(Action) Icon(*EXCLAMATION) Buttons( *YESNOCANCEL )
			
			Select
				  // Cancel
			When Action = 2
				LeaveSR *False
			      // Yes Apply changes first
			When Action = 6
				If Not ApplyChanges()
					LeaveSR *False
				EndIf
				
				// Write out a lock records to DSSRLCK 
				ReApplyLocks()
				       
				// No
			When Action = 7
				LeaveSR *True
			EndSL  
		EndIf
	          
		LeaveSR *True
	EndFunc		
	
	// Write records to DSSRLCK 
	Begsr ReApplyLocks
	
		If Not DSSRLCKL7.IsOpen
			Open DSSRLCKL7
		Endif
		
		toolStripProgressBar1.value = 5
		wApply2 = *False
		          		
		Do  1  *Hival
		  	// Delay until job is complete
			Sleep(1000)
			// increment counter
		    toolStripProgressBar1.Increment(1)     
			
			// Reapply Locks if not there
			ReadRange DSSRLCKL7 FirstKey(12,wToMbr2) Access( *NoLock )
			If Not %Found(DSSRLCKL7)
		        Clear  RDSSRLCK
			
				LODEPT= wDept2
				LOKEY = 12
				LOUSR = As400.User.ToString().Trim().ToUpper() 
				LOMBR = wToMbr2.Trim()
				LODAT = %TimeStamp
				LOMOD = "D"
				
				If rbDaily2.Checked
					LOTYP = "Daily"
				Else
					LOTYP = "Weekly"
				Endif    
	           
				Write RDSSRLCK
			   
				toolStripProgressBar1.Value = toolStripProgressBar1.Maximum
				Leave  
			Endif
		Enddo	
	EndSr
