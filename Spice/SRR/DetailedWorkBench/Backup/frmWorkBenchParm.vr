	// --------------------------------------------------------------------	
	// Set Inherited Value Parms 
	// --------------------------------------------------------------------	
    BegSR  SetInheritedValueParms
		 
		StrMkt   = *Blanks
		StrGrade = *Blanks 
		    
		// BreakDown Item Number    
		CLS = Convert.ToInt32( %Subst(wLongItem#:1:4) ) 
		VEN = Convert.ToInt32( %Subst(wLongItem#:6:5) )
		STY = Convert.ToInt32( %Subst(wLongItem#:12:4) )
		CLR = Convert.ToInt32( %Subst(wLongItem#:17:3) )
		SIZ = Convert.ToInt32( %Subst(wLongItem#:21:4) )
			
		// Set Key Level
		Select
		When wLevel2 = "IS"
			wLevel2# = "1"
			wHierKey# = wStore#.trim()
		        
		When wLevel2 = "IG"
			wLevel2# = "2"
			wHierKey# = wGrade#.trim()
			StrGrade = wGrade#.Trim()
		        
		When wLevel2 = "IM"
			wLevel2# = "3"
			wHierKey# = wMarket#.Trim()
		    StrMkt = wMarket#.Trim()
		        
		When wLevel2 = "IT"
			wLevel2# = "4"
			wHierKey# = *Blank
		EndSL  
		 	   
		// Clear Return Parms   
		NewAlcFlag = *Blank
		NewSmoothF = *Zero
		NewCoverCO = *Zero
		NewUplift  = *Zero
		 	   
		//  Create Instance
		If (ModelRun = *Nothing) 
			ModelRun = *New ModelRun(AS400)
		Endif
    EndSR	

