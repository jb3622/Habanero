	BegSr uxModelRun_Click_1 Access(*Private) Event(*this.uxModelRun.Click)
		DclSrParm sender *Object
		DclSrParm e System.EventArgs
		
		// Confirm action with users
		MsgBox 'Are you sure you want to run a model ?' Buttons(*YesNo) Icon(*Question) +
				DftButton(1) RtnCode(RtnCode)
				
		If RtnCode = 6
		   SetMousePtr *HourGlass
		   *this.Enabled() = *False
		   
		   wApply2 = *False
		   
		   // Check if fileGroup still locked to user
		    If Not DSSRLCKL7.IsOpen
			   Open DSSRLCKL7
			Endif	
       		ReadRange DSSRLCKL7 FirstKey(12,wToMbr2) Access( *NoLock )
			If %Found(DSSRLCKL7)
			   If (LOUSR.trim() <> wAS400User2.trim()) 
				   MsgBox 'File Group is not currently allocated to you. Changes will not be applied'  Icon(*Exclamation)
				   toolStripProgressBar1.Value = 0
				   LeaveSR
			   Endif	   
			 Else
			   MsgBox 'Your File Group has expired. Changes will not be applied'  Icon(*Exclamation)
			   toolStripProgressBar1.Value = 0
			   *this.Enabled() = *True
			   LeaveSR 
		    Endif
		      
		// Disable Grids
			DatagridView1.Visible = *false
			DatagridView2.Visible = *false
			DatagridView3.Visible = *false
			DatagridView4.Visible = *false
		   
			// Process Changes
		    BegProcess()
		  
		// Show Data loaded into Grid 
			DatagridView1.Visible = *true
			DatagridView2.Visible = *true
			DatagridView3.Visible = *true
			DatagridView4.Visible = *true  
		   
		   *this.Enabled() = *True
	    Endif  
    
        // Reset Progress Bar
		toolStripProgressBar1.Value = 0
	EndSr
	
	// ---------------------------------------------------------
	// Process User Changes
	// ---------------------------------------------------------	
	BegSr BegProcess
	
	   DclFld value1 type(*String)
	   DclFld CountRcd Type(*Integer4) 
	   
	   Counter_1 = 0
	   Counter_2 = 0
	   Counter_3 = 0
	   Counter_4 = 0
	   
	   ModelRunActive = *True
	  	   	   
	  // remove Event	   
	   Remove_DataBindingHandler()	
	  	   
       DoEvents
       
		// Set Lever Files
		savItem# = *Blanks
 
        // Clear the Array		
		For index(x=0) to(%Elem(GridChgARR)) 
		    if %Elem(GridChgARR) = x *Or GridChgARR[x] = 000
		       Leave
		    Endif     
		    GridChgARR[x] = 000 
		EndFOR
		
		ArrayIndex = -1
		
		//  Create Instance
		If (ModelRun = *Nothing) 
			ModelRun = *New ModelRun(AS400)
		Endif
		
		//  Create Instance
		If (RunAS400Sql = *Nothing) 
			RunAS400Sql = *New RunAS400Sql(AS400)
		Endif
		
        toolStripProgressBar1.Increment(5)
		
		// Close the lever files so that we dont lock the member...
		closeLeverFiles()
		
		// Create Members
		If Not ModelRun.CreateMembers(wFrmMbr2, wToMbr2, wFileGroup2, wDept2)
			MsgBox 'Members not Created. Contact TDS/IS' Icon(*Exclamation)	
			LeaveSR
		Endif
		
		// Clear out workfile
		wSQLString = 'Delete From QTEMP/DSDWBIW'
		              
		If Not RunAS400Sql.runSQL(' ',' ',' ',' ', wSQLString)
		   MsgBox 'Work File not Cleared. Contact TDS/IS(DS883GC)'  Icon(*Exclamation)
		   LeaveSR
		Endif
									
		// Open Files
		openLeverFiles()
		
		toolStripProgressBar1.Increment(5)
		
		// Update lever files with users changes from the Grid    
		 Do FromVal( 0 ) ToVal( RowsLoaded - 1 ) Index( CountRcd )
		 
		    If CountRcd > RowsLoaded
		       Leave
		    Endif
		       
		    DoEvents
		    value1 = dataGridView1.Item[ 'dRowIndxul', CountRcd ].Value.toString()
		    RecordIndex = System.Convert.ToDecimal( Value1 )
		   	Chain memCustomer Key(RecordIndex)
		    setLeverFiles()
		    
		    // Set Pointer Back...
		    Chain memCustomer Key(RecordIndex)
		Enddo   
	
		toolStripProgressBar1.Increment(20)

        // Clear DSSRDTL member before uploading data.....
        wSQLString = 'Delete From DSSRDTL'   
		If Not RunAS400Sql.runSQL('DSSRDTL',wToMbr2,' ',' ', wSQLString)
		   MsgBox 'DSSRDTL File not Cleared. Contact TDS/IS(DS883GC)'  Icon(*Exclamation)
		   LeaveSR
		Endif
		
		// Copy records from Item Workfile into DSSRDTL Main Member
		wSQLString = 'Insert into DSSRDTL (Select * from DSSRDTL1 a Where Exists (Select * from QTEMP/DSDWBIW b Where a.srdept = b.wdpt and a.srcls = b.wcls and a.srven = b.wven and a.srsty = b.wsty and a.srclr = b.wclr and a.srsiz = b.wsiz))'
		              
		If Not RunAS400Sql.runSQL('DSSRDTL',wToMbr2,'DSSRDTL1',wFrmMbr2, wSQLString)
		   MsgBox 'Items not updated. Contact TDS/IS(DS883GC)'  Icon(*Exclamation)
		   LeaveSR
		Endif
				 	
		// Run Model
		If ModelRun.runModel(wFrmMbr2, wToMbr2, wFileGroup2, wDept2)
		
			// If successfull get Lever totals Stock and Qty
			toolStripProgressBar1.Increment(50)
		
		    // Dont update grid with new values if Apply was clicked	
			If wApply2 = *False
			
				// load DSSRDTL into Memory
				ModelRun.setMemFile(wToMbr2)
	       
				If LeverTotals() 
			      	toolStripProgressBar1.Value = toolStripProgressBar1.Maximum
					If wApply2 = *false
						MsgBox 'Model Run has completed' Buttons(*Ok) Icon(*Information)
					Endif
					
					dataGridView1.Refresh()
					dataGridView2.Refresh()
					dataGridView3.Refresh()
					dataGridView4.Refresh()
					
					// Add the Handler
					Add_DataBindingHandler()
					
					Update MemCustomer
	
				Endif
			Endif	
	
	        // Model Run Complete		   
			ModelRunB = *True
					    	  
			Close DSSRDTL1
		Else
			MsgBox 'Model Run failed. Contact TDS/IS' Icon(*Exclamation) 
		Endif
	EndSr
	
	// ---------------------------------------------------------
	// Update Lever Files
	// ---------------------------------------------------------
	BegSR openLeverFiles
	
		// Open Files 
		If Not DSSRALF.IsOpen
			DSSRALF.MemberName = wToMbr2.Trim()
			Open DSSRALF
		Endif  
		                 
		If Not DSSRCCO.IsOpen
			DSSRCCO.MemberName = wToMbr2.Trim()
			Open DSSRCCO
		Endif  
	     
		If Not DSSRFWF.IsOpen
			DSSRFWF.MemberName = wToMbr2.Trim()
			Open DSSRFWF
		Endif 
	    
		If Not DSSRSMF.IsOpen
			DSSRSMF.MemberName = wToMbr2.Trim()
			Open DSSRSMF
		Endif
	 
		// Open Item WorkFile
		If Not DSDWBIW.IsOpen
		   Open DSDWBIW filePath('QTEMP/DSDWBIW')
		Endif
	EndSR
	
	// ---------------------------------------------------------
	// Close Lever Files
	// ---------------------------------------------------------
	BegSR closeLeverFiles
	
		// Close Files 
		If DSSRALF.IsOpen
		   Close DSSRALF
		Endif  
		                 
		If DSSRCCO.IsOpen
		   Close DSSRCCO
		Endif  
	     
		If DSSRFWF.IsOpen
		   Close DSSRFWF
		Endif 
	    
		If DSSRSMF.IsOpen
		   Close DSSRSMF
		Endif
	            
		// Close SRR Master File
		If DSSRDTL.IsOpen
		   Close DSSRDTL
		Endif
		
		// Close Item WorkFile
		If DSDWBIW.IsOpen
		   Close DSDWBIW
		Endif
		 
	EndSR
	
	// ---------------------------------------------------------
	// Update Lever Files
	// ---------------------------------------------------------
	BegSR SetLeverFiles
	
		DclFld CLS  Like(CCCLS)
		DclFld VEN  Like(CCVEN)
		DclFld STY  Like(CCSTY)
		Dclfld CLR  Like(CCCLR)
		DclFld SIZ  Like(CCSIZ)
		DclFld LKey Like(CCKYVL)
		
		DclFld wIdx  Type(*integer4)
		DclFld Item# Type(*Char) Len(24)
	     
		Changed2 = *False
	     
	    CLS = Convert.ToInt32( %Subst(M_ITEM: 1:4) ) 
		VEN = Convert.ToInt32( %Subst(M_ITEM: 6:5) )
		STY = Convert.ToInt32( %Subst(M_ITEM:12:4) )
		CLR = Convert.ToInt32( %Subst(M_ITEM:17:3) )
		SIZ = Convert.ToInt32( %Subst(M_ITEM:21:4) )
		
		// ---------------------------------------------------------
		// Set Key Level
		// ---------------------------------------------------------
		Select
		When wLevel2 = "IS"
			wLevel2N = 1
			LKey = M_Store.toString().trim()
	        
		When wLevel2 = "IG"
			wLevel2N = 2
			LKey = M_GRADE.trim()
	        
		When wLevel2 = "IM"
			wLevel2N = 3
			LKey = M_MARKET.trim()
	        
		When wLevel2 = "IT"
			wLevel2N = 4
			LKey = *Blanks
		EndSL  
	 
		// Set Data 
		If M_LCKUSER <> *Blanks
			LeaveSR
		Endif   
			
		// Set As Changed		
		If M_OLDUPLFT = 'C' *OR M_OLDCOCUO = 'C' *OR M_OLDCALFL = 'C' *OR M_OLDSMFCT = 'C'
			Changed2 = *True
		Endif
		    	
		// ---------------------------------------------------------	
		// Uplift Factor
		// ---------------------------------------------------------
		If M_OLDUPLFT = 'C' 
			Changed2 = *True
			Clear RDSSRFWF 
			ReadRange DSSRFWF  FirstKey(wLevel2N,wDept2,CLS,VEN,STY,CLR,SIZ,LKey)
			If DSSRFWF.IsFound
				If M_DLTRCDU <> *Blanks
					Delete RDSSRFWF
				Else 
					FFFFCT = System.Convert.ToDecimal( M_NEWUPLFT )
					FFCHGU = as400.User.toUpper().Trim()
					FFCHGD = %Time
					FFAPRU = 'DWBC'
					Update RDSSRFWF
				Endif    
			ElseIf  M_NEWUPLFT <> *Blanks *And M_DLTRCDU = *Blanks
				FFKEY = wLevel2N.toString().trim() 
				FFDEPT= wDept2
				FFCLS = CLS
				FFVEN = VEN 
				FFSTY = STY 
				FFCLR = CLR 
				FFSIZ = SIZ 
				FFSTR = M_STORE
				FFGRD = M_GRADE
				FFMKT = M_MARKET
				FFFFCT = System.Convert.ToDecimal( M_NEWUPLFT)
				FFWFLG = *Blanks
				FFCHGU = as400.User.toUpper().Trim()
				FFCHGD = %TimeStamp
				FFAPRU = 'DWBW'
				FFAPRD = *Loval
				FFKYVL = LKey.trim()
										
				Write RDSSRFWF
			Endif
		Endif
        
        // --------------------------------------------------------- 
        // Cover Cutoff   
        // ---------------------------------------------------------
		If  M_OLDCOCUO = 'C'  
			Changed2 = *True	
			Clear RDSSRCCO
			ReadRange DSSRCCO  FirstKey(wLevel2N,wDept2,CLS,VEN,STY,CLR,SIZ,LKey)
			If DSSRCCO.IsFound
				If M_DLTRCDF <> *Blanks
					Delete RDSSRCCO
				Else 
					CCCCO = System.Convert.ToDecimal( M_NEWCOCUO )
					CCCHGU = as400.User.toUpper().Trim()
					CCCHGD = %TimeStamp
					CCAPRU = 'DWBC'
					
					Update RDSSRCCO
				Endif     
			ElseIf M_NEWCOCUO <> *Blanks *And M_DLTRCDF = *Blanks
				CCKEY = wLevel2N.toString().trim() 
				CCDEPT= wDept2
				CCCLS = CLS 
				CCVEN = VEN 
				CCSTY = STY 
				CCCLR = CLR 
				CCSIZ = SIZ 
				CCSTR = M_STORE
				CCGRD = M_GRADE
				CCMKT = M_MARKET
				CCCCO = System.Convert.ToDecimal( M_NEWCOCUO )
				CCWFLG = *Blanks
				CCCHGU = as400.User.toUpper().Trim()
				CCCHGD = %TimeStamp
				CCAPRU = 'DWBW'
				CCAPRD = *Loval
				CCKYVL = LKey.trim()
								
				Write RDSSRCCO
			Endif	
		Endif
           
           // ---------------------------------------------------------
           // Allocation Flag
           // ---------------------------------------------------------
			If  M_OLDCALFL = 'C'  
				Changed2 = *True	 
				Clear RDSSRALF
				
				Chain DSSRALF  Key(wLevel2N,wDept2,CLS,VEN,STY,CLR,SIZ,LKey)
				If DSSRALF.IsFound
					If M_DLTRCDA <> *Blanks
						Delete RDSSRALF  	
					Else 
						AFAFLG = M_NEWCALFL
						AFCHGU = as400.User.toUpper().Trim()
						AFCHGD = %TimeStamp
						AFAPRU = 'DWBC'
						
						Update RDSSRALF
					Endif     
				ElseIf M_NEWCALFL <> *Blanks *And M_DLTRCDA = *Blanks
					AFKEY  = wLevel2N.toString().trim() 
					AFDEPT = wDept2
					AFCLS  = CLS 
					AFVEN  = VEN 
					AFSTY  = STY 
					AFCLR  = CLR 
					AFSIZ  = SIZ 
					AFSTR  = M_STORE
					AFGRD  = M_GRADE
					AFMKT  = M_MARKET
					AFAFLG = M_NEWCALFL
					AFWFLG = *Blanks
					AFCHGU = as400.User.toUpper().Trim()
					AFCHGD = %TimeStamp
					AFAPRU = 'DWBW'
					AFAPRD = *Loval
					AFKYVL = LKey.trim()
										
					Write RDSSRALF 
				Endif	
			Endif
        
            // ---------------------------------------------------------
			// Smooth Factor
			// ---------------------------------------------------------
			If  M_OLDSMFCT = 'C'
				Changed2 = *True 
				Clear RDSSRSMF
				ReadRange DSSRSMF  FirstKey(wLevel2N,wDept2,CLS,VEN,STY,CLR,SIZ,LKey)
				If DSSRSMF.IsFound
					If M_DLTRCDS <> *Blanks
						  Delete RDSSRSMF  	
					Else 
						  SFSFCT = System.Convert.ToDecimal( M_NEWSMFCT  )
						  SFCHGU = as400.User.toUpper().Trim()
						  SFCHGD = %TimeStamp
						  SFAPRU = 'DWBC'
						  Update RDSSRSMF
					Endif   
				ElseIf M_NEWSMFCT <> *Blanks *And M_DLTRCDS = *Blanks
					SFKEY = wLevel2N.toString().trim() 
					SFDEPT= wDept2
					SFCLS = CLS
					SFVEN = VEN 
					SFSTY = STY 
					SFCLR = CLR 
					SFSIZ = SIZ 
					SFSTR = M_STORE
					SFGRD = M_GRADE
					SFMKT = M_MARKET
					SFSFCT = System.Convert.ToDecimal( M_NEWSMFCT )
					SFWFLG = *Blanks
					SFCHGU = as400.User.toUpper().Trim()
					SFCHGD = %TimeStamp
					SFAPRU = 'DWBW'
					SFAPRD = *Loval
					SFKYVL = LKey.trim()
											
					Write RDSSRSMF  
				Endif
			Endif
	    
		// No Changes
		If Changed2 = *False
		   LeaveSR
		Endif 
	    
	    If wApply2 = *false
			item# = M_ITEM.trim()
		    
			// Set rows as changed so rationing can take place
			  Do fromVal(0) toVal(RowsLoaded)  Index(widx) 
				 Chain memCustomer Key(widx)
         		 If %Eof(memCustomer)
         			 Leave
         		 Endif  
	         	      
         //		 If M_Item.trim() <> Item#.Trim() And wfound = *True
         //			Leave
         //		 Endif
	         	       
				 If M_Item.trim() = Item#.Trim() 
					wFound = *True
					If M_OLDSMFCT <> 'C' 
					   M_OLDSMFCT = '*' 
					Endif
					If M_OLDUPLFT  <> 'C'    
					   M_OLDUPLFT = '*'
					Endif
					If M_OLDCOCUO <> 'C'    
					   M_OLDCOCUO = '*'
					Endif
					If M_OLDCALFL <> 'C'    
					   M_OLDCALFL = '*'
					Endif   
					Update WRKBNCHR
				  Endif		         
			 Enddo  
		Endif	 
	    
		// Store the rrn number from the memory file in the array,  so i've got a pointer to 
		// where in the grid the user made a change
		ArrayIndex += 1
		GridChgARR[ArrayIndex] = RecordIndex
	
		// ---------------------------------------------------------
		// DSDWBIW - Item WorkFile used to map SQL statement against
		// ---------------------------------------------------------
	  	Chain DSDWBIW Key(wDept2,CLS,VEN,STY,CLR,SIZ) Access(*NoLock) 
		If Not DSDWBIW.IsFound
			Clear  RDSDWBI
			wDPT = wDept2
			wCLS = CLS
			wVEN = VEN
			wSTY = STY
			wCLR = CLR
			wSIZ = SIZ 
			Write RDSDWBI Err(*Extended)
	    Endif
	EndSR     
	
	// ---------------------------------------------------------
	// Set Lever Totals Back to the grid
	// ---------------------------------------------------------
	BegFunc LeverTotals  Type(*Boolean)
	    DclFld M_TOTSTCKW  Type(*Zoned)  Len(7,0)
		DclFld M_MDLREQTW  Type(*Zoned)  Len(7,0)
		DclFld M_NEWSROSW  Type(*Zoned)  Len(6,2)
		DclFld M_PRALQTYW  Type(*Zoned)  Len(7,0)
	
		*this.SuspendLayout()
		
