/Region APP processing
	BegSr reviewAPAllocationToolStripMenuItem_Click Access(*Private) Event(*this.reviewAPAllocationToolStripMenuItem.Click)
		DclSrParm sender *Object
		DclSrParm e System.EventArgs
		
		DclFld Return Type(Int16)
		
		DclFld PackClass  Type(*Zoned) Len(4,0)
		DclFld PackVendor Type(*Zoned) Len(5,0)
		DclFld PackStyle  Type(*Zoned) Len(4,0)
		DclFld PackColour Type(*Zoned) Len(3,0)
		DclFld PackSize   Type(*Zoned) Len(4,0)
		
		// Test for changes to be saved first (If any)
//		MsgBox 'Do you wish to Apply any changes ?' Buttons(*YesNo) Icon(*Question) +
//				DftButton(1) RtnCode(Return)
				
		//If Return = 6
			// Rebuild grid without drill but with apply
			ApplyChgBeforeAPP2()
		//EndIf

		If not DSIALOK.IsOpen
			Open DSIALOK
		EndIf

		PackClass  = PackItem.SubString(0,4)
		PackVendor = PackItem.SubString(5,5)
		PackStyle  = PackItem.SubString(11,4)
		PackColour = PackItem.SubString(16,3)
		PackSize   = PackItem.SubString(20,4)
		Chain From(DSIALOK) Key(PackClass,PackVendor,PackStyle,PackColour,PackSize) Access(*NoLock)
		If DSIALOK.IsFound
			MsgBox 'APP is being reviewed by ' + IAJOB.Trim() + ' Please try later...'
			LeaveSR
		Else
			IACLS  = PackClass
			IAVEN  = PackVendor
			IASTY  = PackStyle
			IACLR  = PackColour
			IASIZ  = PackSize
			IAUSER = *Blank
			IAFGP  = *Zero
			IAJOB  = AS400.User.ToUpper()
			IADTTM = DateTime.Now
			Write IALOK
		EndIf

		ApRelease = *New Disney.SRR.frmSRRrelease(AS400, PackItem, *this, wToMbr2, wFrmMbr2)
		
		Try
			DialogResult = ApRelease.ShowDialog()
		Catch Name(Err) Type(System.Exception)
			MsgBox Err.ToString()
		EndTry
		
		//If DialogResult <> DialogResult.Cancel
			LoadData()
		//EndIf
		
		*this.DialogResult = DialogResult.None
		ApRelease = *Nothing
		
		If not DSIALOK.IsOpen
			Open DSIALOK
		EndIf		
		Delete From(DSIALOK) key(PackClass,PackVendor,PackStyle,PackColour,PackSize)
	EndSr
	
	BegSr CloseFilesForAPP Access(*Public)
	
		Select  
		When wLevel2 = "IT" 
			If DSSRWBIT.IsOpen
				Close DSSRWBIT
			Endif	
		When wLevel2 = "IS" 
			If DSSRWBIS.IsOpen
				Close DSSRWBIS
			Endif
		When wLevel2 = "IM" 
			If DSSRWBIMB.IsOpen
				Close DSSRWBIMB
			Endif
		When wLevel2 = "IG" 
			If DSSRWBIGB.IsOpen
				Close DSSRWBIGB
			Endif
		EndSL
	EndSr
	
	BegSr ApplyChgBeforeAPP Access(*Private)
		
		wApply2 = *True
		toolStripProgressBar1.Value = 5
		    
		// Check if fileGroup still locked to user
		If Not DSSRLCKL7.IsOpen
			Open DSSRLCKL7
		Endif	
		
       	ReadRange DSSRLCKL7 FirstKey(12,wToMbr2) Access( *NoLock )
		If %Found(DSSRLCKL7)
			If (LOUSR.trim() <> wAS400User2.trim()) 
				MsgBox 'File Group is not currently allocated to you. Changes will not be applied' Icon(*Exclamation)
				toolStripProgressBar1.Value = 0
				LeaveSR
			Endif	   
		Else
			MsgBox 'Your File Group has expired. Changes will not be applied'  Icon(*Exclamation)
			toolStripProgressBar1.Value = 0
			LeaveSR
		Endif
		   
		// Process Changes
		BegProcess()
		 
		toolStripProgressBar1.Value = 15
	       
		// Call Wrapper Program   
		If Not ModelRun.ApplyChanges(wFrmMbr2, wToMbr2, wFileGroup2, wDept2)
			MsgBox 'Model Run Failed. Contact TDS/IS' Icon(*Exclamation)
			toolStripProgressBar1.Value = 0
			LeaveSR
		Endif    
			
		// Exit Process
		toolStripProgressBar1.Value = toolStripProgressBar1.Maximum
		ModelRunB = *False

		ReApplyLocks()
		
		// Confirm Success
		MsgBox 'Changes Submitted Successfully' Icon(*INFORMATION ) 
		toolStripProgressBar1.Value = 0   		
	EndSR
	
	BegSr ApplyChgBeforeAPP2 Access(*Private)
	
		If CheckChanges() = *False
		   toolStripProgressBar1.value = 0   
		   LeaveSR
		Endif
		
		// Set New Level
		wGrade2 = wGrade2_Bk
		wMarket2 = wMarket2_Bk
		wStore2 = wStore2_Bk 
		ShowAll = *True
		
		// Set SQL String then reload memory file & Grid
		ReBuildGrid()
		ShowAll = *False
	EndSr
/EndRegion		
EndClass