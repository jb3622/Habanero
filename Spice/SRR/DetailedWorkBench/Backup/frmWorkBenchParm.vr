	// --------------------------------------------------------------------
	// If record is changed update file - Smooth Factor
	// --------------------------------------------------------------------	
	BegSr dataGridView4_CellValueChanged Access(*Private)
		DclSrParm sender *Object
		DclSrParm e System.Windows.Forms.DataGridViewCellEventArgs
		
		DclFld Column Type(*Integer4)
		DclFld Row    Type(*Integer4)
		
		DclFld dgvr      Type( DataGridViewRow )
		DclFld wIdx1     Type( *integer4 )
		DclFld wSmoothFactor Type(*Char) Len(5) 
		DclFld wSmoothFactorN Type(*Zoned) Len(5,2)
		DclFld wCurSmoothFactorN Type(*Zoned) Len(5,2)
		DclFld CurLevel    Type( *Char ) len(1)
		
		DclFld wMemIndxsm   Type(*Zoned)  Len(5,0)
		DclFld wMemIndxsm#  Type(*Char)   Len(5)
		
		Column = e.ColumnIndex
		Row    = e.RowIndex
		
		If RowsLoaded <=0
		  LeaveSR
		Endif
		
		Counter_1 = 0
		Counter_2 = 0
		Counter_3 = 0
		Counter_4 = 0
	    ModelRunActive = *false	
	    wfound = *false
 	 
		
		// Stop a recursive loop
		If Changed = *True
		   Changed = *False
		   LeaveSR
		Endif  
		
		// Remove the binding handler
		Remove_DataBindingHandler()
	
		// Reset Fields
		DeleteRecord = *Blanks
		CurLevel = *Blanks
		
		
		// Set Changed cell font to Red	 
		dataGridView4.item['smoothFactor',Row].Style.ForeColor = Color.Red
	    		
		// Set data that has been changed	
				
		dgvr = dataGridView4.Rows[Row]
		
		wLongItem#			= dgvr.Cells["dItemSm"].Value.ToString()
		wStore#				= dgvr.Cells["dStoreSF"].Value.ToString()
		wMarket#			= dgvr.Cells["dMarketSF"].Value.toString()
		wGrade#             = dgvr.Cells["dGradeSF"].Value.toString()  
		wCurSmoothFactorN	= dgvr.Cells["dCursm"].Value.ToString()
		wMemIndxsm#			= dgvr.Cells["dRowIndxsm"].Value.ToString()
		wUserLock			= dgvr.Cells["dLockUsersm"].Value.ToString()
		CurLevel			= dgvr.Cells["dCurLevelSmooth"].Value.ToString()
		
		wMemIndxSm = System.Convert.ToDecimal( wMemIndxsm# )
		
		Try
            wSmoothFactor	= dgvr.Cells["smoothFactor"].Value.ToString()
	      Catch type( System.Exception )
	        wSmoothFactor = *Blanks
	    EndTry
		
		// Clear Errors
	  	If dataGridView4.Item[Column,Row].ErrorText <> String.Empty
		   ErrorCount -= 1
		   dataGridView4.Item[Column,Row].ErrorText = String.Empty
		EndIf
		
		// Reset Error
		If ErrorCount < 0
		   ErrorCount = 0
		Endif   
		
		If  wSmoothFactor = *Blanks
		 //   ErrorCount -= 1
		    wSmoothFactorN = 0
		    
		     // Set the call Parms
		    SetInheritedValueParms()
		    
			// Retrieve inherited value
			If ModelRun.RtvInheritedValue(wDept2,'M',CLS,VEN,STY,CLR,SIZ,wLevel2#,wHierKey#,*ByRef NewAlcFlag,*ByRef NewCoverCO,*ByRef NewUplift,*ByRef NewSmoothF,wFrmMbr2,StrMkt,StrGrade,' ',' ',' ',' ')
			//   Changed = *True  
			   CloseDS883 = *True
			  
			   // Delete Record from Lever File 
			 //  If CurLevel = 'Y'
				  DeleteRecord = 'Y'
				  dataGridView4.item['smoothFactor',Row].Style.ForeColor = Color.Blue	
			  // Endif
			   wSmoothFactor = NewSmoothF.toString()	
			   wSmoothFactorN = System.Convert.ToDecimal( wSmoothFactor )
			Endif
		    
	// *************** 
		 Else   
	// ***************                         
    // Validate Field
          Try
            wSmoothFactorN = System.Convert.ToDecimal( wSmoothFactor ) 	
          
	       Catch type( System.Exception )
	         dataGridView4.Item[Column,Row].ErrorText = 'Value must be Numeric'
						ErrorCount += 1
	       EndTry
	   
		   // Smooth factor cannot be less then 0 or > 1 
		   if  wSmoothFactorN > 1.00  *Or wSmoothFactorN < 0.00 
	  			dataGridView4.Item[Column,Row].ErrorText = 'Value must not be greater then 1'
				ErrorCount += 1
		   Endif
	   
	   Endif
	                     
	   // Increment counter
