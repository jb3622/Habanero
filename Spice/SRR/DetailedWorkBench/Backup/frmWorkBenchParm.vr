/Region Apply
	// copy member back into DLOAD or WELOAD	
	BegSr uxApply_Click Access(*Private) Event(*this.uxApply.Click)
		DclSrParm sender *Object
		DclSrParm e System.EventArgs
			    
	    // Confirm action with users
		MsgBox 'Are you sure you want to Apply Changes ?' Buttons(*YesNo) Icon(*Question) +
				DftButton(1) RtnCode(RtnCode)
				
		If RtnCode = 6
			SetMousePtr *HourGlass
			*this.Enabled() = *False
			
			// Apply Changes
			If Not ApplyChanges()
			   *this.Enabled() = *True
			   wApply2 = *False
			   LeaveSR
			Endif
			
			*this.Enabled() = *False
			
			// Close
			CloseFiles()
			Unload *this
		Endif  
	EndSr
		
	BegFunc ApplyChanges  Type(*Boolean)  Access(*Public)
	
		wApply2 = *True
		toolStripProgressBar1.Value = 5
		    
		// Check if fileGroup still locked to user
		If Not DSSRLCKL7.IsOpen
			Open DSSRLCKL7
		Endif	
		
       	ReadRange DSSRLCKL7 FirstKey(12,wToMbr2) Access( *NoLock )
		If %Found(DSSRLCKL7)
			If (LOUSR.trim() <> wAS400User2.trim()) 
				MsgBox 'File Group is not currently allocated to you. Changes will not be applied'  Icon(*Exclamation)
				toolStripProgressBar1.Value = 0
				LeaveSR *False
			Endif	   
		Else
			MsgBox 'Your File Group has expired. Changes will not be applied'  Icon(*Exclamation)
			toolStripProgressBar1.Value = 0
			LeaveSR *False
		Endif
		          
		// Disable Grids
		DatagridView1.Visible = *false
		DatagridView2.Visible = *false
		DatagridView3.Visible = *false
		DatagridView4.Visible = *false
		   
		// Process Changes
		BegProcess()
		 
		toolStripProgressBar1.Value = 15
	       
		// Call Wrapper Program   
		If Not ModelRun.ApplyChanges(wFrmMbr2, wToMbr2, wFileGroup2, wDept2)
			MsgBox 'Model Run Failed. Contact TDS/IS' Icon(*Exclamation)
			toolStripProgressBar1.Value = 0
			LeaveSR *False
		Endif    
			
		// Exit Process
		toolStripProgressBar1.Value = toolStripProgressBar1.Maximum
		ModelRunB = *False
	   
		// Confirm Success
		MsgBox 'Changes Submitted Successfully' Icon(*INFORMATION ) 
		toolStripProgressBar1.Value = 0   

        LeaveSR *True
	EndFunc
/EndRegion
    
