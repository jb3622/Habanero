	BegSr dataGridView1_CellFormatting Access(*Private) Event(*this.dataGridView1.CellFormatting)
		DclSrParm sender *Object
		DclSrParm e System.Windows.Forms.DataGridViewCellFormattingEventArgs
		
		DclFld theRow Type(DataGridViewRow)  
		theRow = dataGridView1.Rows[e.rowIndex]
						
		if theRow.Cells["StrType"].Value.ToString() = 'B&M' and theRow.Cells["WrkBnch"].Value.ToString() = 'WEEK'
			theRow.DefaultCellStyle.BackColor = Color.LightSalmon
		Endif
		
		if theRow.Cells["StrType"].Value.ToString() = 'B&M' and theRow.Cells["WrkBnch"].Value.ToString() = 'DAY'
			theRow.DefaultCellStyle.BackColor = Color.Chocolate
		Endif
		
		if theRow.Cells["StrType"].Value.ToString() = 'ONLINE' and theRow.Cells["WrkBnch"].Value.ToString() = 'WEEK'
			theRow.DefaultCellStyle.BackColor = Color.PaleTurquoise
		Endif
		
		if theRow.Cells["StrType"].Value.ToString() = 'ONLINE' and theRow.Cells["WrkBnch"].Value.ToString() = 'DAY'
			theRow.DefaultCellStyle.BackColor = Color.Teal
		Endif 
		
	EndSr
	
