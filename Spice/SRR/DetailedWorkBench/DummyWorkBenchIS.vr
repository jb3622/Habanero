	BegSr PopulateTable Access(*Private)
	
		DclFld dr Type(DataRow)
		
		If Not DSSRWBISMW.IsOpen
			Open DSSRWBISMW
		EndIf
		
		DoUntil DSSRWBISMW.IsEof
			Read DSSRWBISMW
			If Not DSSRWBISMW.IsEof
				dr = Table1.NewRow()
				
				dr.Item["Item"]							= ISCLS + '-' + ISVEN + '-' + ISSTY + '-' + ISCLR.ToString("000") + '-' + ISSIZ.ToString("0000")
				dr.Item["Description"]					= ISIDES
				dr.Item["Store"]                        = ISSTR
				dr.Item["StoreName"]					= ISSNAM
				dr.Item["StrType"]						= ISSTYP
				dr.Item["WrkBnch"]						= ISWKB 
				dr.Item["Sales LW"]						= ISSLUL
				dr.Item["Sales TW"]						= ISSLUT
				dr.Item["Smoothed Rate of Sale"]		= ISCSRS
				dr.Item["New Smoothed Rate of Sale"]	= ISSRSL  
				dr.Item["Store SoH"]					= ISSSOH 
				dr.Item["Total Store Cover"]			= ISSWOC
				dr.Item["OH Store Cover"]				= ISOHSCOV
				dr.Item["Total Stock Required"]			= ISCSSR
				dr.Item["Give back Y/N"]				= ISGIVB
				dr.Item["Ringfence"]					= ISRINGF
				dr.Item["Allocated"]					= ISINTA
				dr.Item["Shipped"]						= ISINTS
				dr.Item["Min Display Qty"]				= ISMDQT
				dr.Item["Ideal Stock Req Qty"]			= ISNSRQ
				dr.Item["Prop. Alloc Qty"]				= ISPRAQ
				dr.Item["EDC Stock"]					= ISESOH
				dr.Item["APP Qty"]						= ISAPPQ
				dr.Item["Smoothed EDC Cover"]			= ISEWOC
				dr.Item["Next Delivery Date"]			= ISNODT
				dr.Item["Next Order Qty"]				= ISNOQT
				dr.Item["Current Pattern"]				= ISITPA
				dr.Item["New Pattern"]					= ISNPAT
				dr.Item["Current Uplift"]				= ISCFWF
				dr.Item["New Uplift Factor"]			= ISNFWF
				dr.Item["Current Cover C/O"]			= ISCCCO
				dr.Item["New Cover C/O"]                = ISNCCO
				dr.Item["Current Alloc Flag"]			= ISCAFL
				dr.Item["New Alloc Flag"]				= ISNAFL
				dr.Item["Current Smooth Factor"]		= ISSFCT 
				dr.Item["New Smooth Factor"]            = ISNSMF 
				dr.Item["Days Out of Stock"]			= ISSTOU
				dr.Item["Locked User"]					= ISAFLLOCK
				dr.Item["AP Master Item Codek"]			= ISAPMST
				dr.Item["Lever Level"]					= ISLVRLV
								
				Table1.Rows.Add(dr)
			EndIf
		EndDo
				
		
		Close DSSRWBISMW
	EndSr
		
