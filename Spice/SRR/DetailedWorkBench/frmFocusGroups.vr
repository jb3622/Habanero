	BegSR FastCvrLines
		
		DclFld LastNbr  Type(*Zoned) Len(9,0) Inz(*Zero)
		DclFld FirstNbr Type(*Zoned) Len(9,0) Inz(*Zero)
		
		Select
		When rbItemMarket.Checked
			DSSRWBIMA.MemberName = MemberName.Trim()
			Open DSSRWBIMA
			
			SetLL DSSRWBIMA Key(wDepartment)
			ReadE DSSRWBIMA Key(wDepartment)
			If Not DSSRWBIMA.IsEof
				If IMCLRK <> *Zero
					LastNbr = IMCLRK
					FirstNbr = LastNbr - (NumberOf - 1)
					If FirstNbr < *Zero
						FirstNbr = *Zero
					EndIf
				EndIf
			EndIf
			
			Close DSSRWBIMA
		
			QrySelect = 'IMCLRK = %RANGE(' + FirstNbr.ToString() + ' ' + LastNbr.ToString() + +
		                ') *And IMDPT = ' + wDepartment.ToString()
		
			QryKey = 'IMCLRK *DESCEND'
			Level  = 'IM'
			
		When rbItemGrade.Checked
			DSSRWBIGA.MemberName = MemberName.Trim()
			Open DSSRWBIGA
			
			SetLL DSSRWBIGA Key(wDepartment)
			ReadE DSSRWBIGA Key(wDepartment)
			If Not DSSRWBIGA.IsEof
				If IGCSRK <> *Zero
					LastNbr = IGCSRK
					FirstNbr = LastNbr - (NumberOf - 1)
					If FirstNbr < *Zero
						FirstNbr = *Zero
					EndIf
				EndIf
			EndIf
			
			Close DSSRWBIGA
			
			QrySelect = 'IGCLRK = %RANGE(' + FirstNbr.ToString() + ' ' + LastNbr.ToString() + +
		                ') *And IGDPT = ' + wDepartment.ToString()
		
			QryKey = 'IGCLRK *DESCEND'
			Level  = 'IG'
			
		When rbItemStore.Checked
			DSSRWBISA.MemberName = MemberName.Trim()
			Open DSSRWBISA
			
			SetLL DSSRWBISA Key(wDepartment)
			ReadE DSSRWBISA Key(wDepartment)
			If Not DSSRWBISA.IsEof
				If ISCLRK <> *Zero
					LastNbr = ISCLRK
					FirstNbr = LastNbr - (NumberOf - 1)
					If FirstNbr < *Zero
						FirstNbr = *Zero
					EndIf
				EndIf
			EndIf
			
			Close DSSRWBISA
			
			QrySelect = 'ISCLRK = %RANGE(' + FirstNbr.ToString() + ' ' + LastNbr.ToString() + +
		                ') *And ISDPT = ' + wDepartment.ToString()
			
			QryKey = 'ISCLRK *DESCEND'
			Level  = 'IS'
			
		When rbItem.Checked
			DSSRWBITA.MemberName = MemberName.Trim()
			Open DSSRWBITA
			
			SetLL DSSRWBITA Key(wDepartment)
			ReadE DSSRWBITA Key(wDepartment)
			If Not DSSRWBITA.IsEof
				If ITCLRK <> *Zero
					LastNbr = ITCLRK
					FirstNbr = LastNbr - (NumberOf - 1)
					If FirstNbr < *Zero
						FirstNbr = *Zero
					EndIf
				EndIf
			EndIf
			
			Close DSSRWBITA
			
			QrySelect = 'ITCLRK = %RANGE(' + FirstNbr.ToString() + ' ' + LastNbr.ToString() + +
		                ') *And ITDPT = ' + wDepartment.ToString()
			
			QryKey = 'ITCLRK *DESCEND'
			Level  = 'IT'
		EndSL
		
		InstanceForm()
	EndSR
	
