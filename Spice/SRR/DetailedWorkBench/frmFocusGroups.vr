	BegSR frmFocusGroups_Load Access(*Private) Event(*this.Load)
		DclSrParm sender *Object
		DclSrParm e System.EventArgs
		
		
		// Loadup list of markets into Combo list box
		Open DSCNTAP3
		cboListBoxMkts.Items.Clear()
		
		DoUntil DSCNTAP3.IsEof
			Read DSCNTAP3
			If Not DSCNTAP3.IsEof And CSBGRP <> *Blank
				cboListBoxMkts.Items.Add(CSCODE.toString() + ' - ' + CSDESC, *True)
			EndIf
		EndDo
		
		Close DSCNTAP3
		
		If Not IPCLASS.IsOpen
			Open IPCLASS
		EndIf
        
		rbNewLines.Checked     = *On
		GroupBox2.Visible      = *On
		GroupBox4.Visible      = *Off
		cboAllMkts.Checked     = *On
		cboListBoxMkts.Enabled = *Off
		tkExclEDCzero.Enabled  = *On

		txtNewLines.Text     = 4
		txtFastCvrStr.Text   = 10
		txtSlowCvrStr.Text   = 10
		txtFastCvrLines.Text = 20
		txtSlowCvrLines.Text = 20
		txtTopLines.Text     = 20
		
		txtFileGroup.Text  = wFileGroup.toString()
		txtUser.Text       = As400.User().toUpper().trim()
		txtDepartment.Text = wDepartment.ToString('000')
		lblCycle.Text      = LoTyp
		
		txtItemCls.text = String.Empty
		txtItemVen.text = String.Empty
		txtItemSty.text = String.Empty
		txtItemClr.text = String.Empty
		txtItemSiz.text = String.Empty
	EndSr

	BegSR btnContinue_Click Access(*Private) Event(*this.btnContinue.Click)
		DclSrParm sender *Object
		DclSrParm e System.EventArgs
		
		DclFld ErrorCount Type(*Integer2)
		DclFld @Ven Type(*Zoned) Len(5,0)
		DclFld @Sty Type(*Zoned) Len(4,0)
		DclFld @Clr Type(*Zoned) Len(3,0)
		DclFld @Siz Type(*Zoned) Len(4,0)
		
		ErrorProvider1.Clear()
		
		If rbFastCvrStr.Checked Or rbSlowCvrStr.Checked
			If cboAllMkts.Checked = *Off And cboListBoxMkts.CheckedItems.Count = *Zero
				errorProvider1.SetError( cboListBoxMkts, 'Select at least One Country' )
				LeaveSR
			EndIf
		Else
			ErrorCount = *Zero

			If (txtItemCls.Text = String.Empty) *And txtItemVen.Text <> String.Empty
				errorProvider1.SetError( txtItemCls, "Please enter Class" )
				ErrorCount += 1
			EndIf
		
        
			 // Check Class is Valid
			If txtItemCls.text <> String.Empty   
				Try
					CCLS = Convert.ToInt32(txtItemCls.Text)
					Chain IPCLASS Key(CCLS)
					If (IPCLASS.IsFound)
						If CDPT <> wDepartment
							ErrorProvider1.SetError( txtItemCls, "Class not Found in Department: " + wDepartment.ToString('000') )
							ErrorCount += 1
						Endif   
					Else         
						ErrorProvider1.SetError( txtItemCls, "Class not Found")
						ErrorCount += 1
					Endif
					
				Catch type( System.Exception )
					ErrorProvider1.SetError( txtItemCls, "Class is Invalid")
					ErrorCount += 1
				Endtry  
			EndIf
		
			If txtItemVen.text <> String.Empty
				Try
					@Ven = Convert.ToInt32(txtItemVen.Text)
				Catch Err Type(System.Exception)
					ErrorProvider1.SetError( txtItemVen, "Number is Invalid")
					ErrorCount += 1
				EndTry
			EndIf
			
			If txtItemSty.text <> String.Empty
				Try
					@Sty = Convert.ToInt32(txtItemSty.Text)
				Catch Err Type(System.Exception)
					ErrorProvider1.SetError( txtItemSty, "Number is Invalid")
					ErrorCount += 1
				EndTry
			EndIf
		
			If txtItemClr.text <> String.Empty
				Try
					@Clr = Convert.ToInt32(txtItemClr.Text)
				Catch Err Type(System.Exception)
					ErrorProvider1.SetError( txtItemClr, "Number is Invalid")
					ErrorCount += 1
				EndTry
			EndIf
			
			If txtItemSiz.text <> String.Empty
				Try
					@Siz = Convert.ToInt32(txtItemSiz.Text)
				Catch Err Type(System.Exception)
					ErrorProvider1.SetError( txtItemSiz, "Number is Invalid")
					ErrorCount += 1
				EndTry
			EndIf
		
			If ErrorCount <> *Zero
				LeaveSR
			EndIf
		EndIf
		
		// Show full price lines and show markdown lines...One must be checked 
		If tkShowFP.Checked = *False  *And  tkShowMD.Checked = *False
			errorProvider1.SetError( tkShowFP, "One Selection must be made")
			errorProvider1.SetError( tkShowMD, "One Selection must be made")
			LeaveSR
		Endif
		
		Select
		When rbNewLines.Checked
		
			// Validate txtNewLines and Convert to numeric value
			ErrorProvider1.SetError(txtNewLines, String.Empty)
			Try
				NumberOf = Convert.ToInt32(txtNewLines.Text)
			Catch Err Exception
				ErrorProvider1.SetError(txtNewLines, 'Invalid number')
				LeaveSR
			EndTry
			
			NewLines()
		
		When rbFastCvrStr.Checked
			ErrorProvider1.SetError(txtFastCvrStr, String.Empty)
			Try
				NumberOf = Convert.ToInt32(txtFastCvrStr.Text)
			Catch Err Exception
				ErrorProvider1.SetError(txtFastCvrStr, 'Invalid number')
				LeaveSR
			EndTry
			
			FastCvrStr()
		
		When rbFastCvrLines.Checked
			ErrorProvider1.SetError(txtFastCvrLines, String.Empty)
			Try
				NumberOf = Convert.ToInt32(txtFastCvrLines.Text)
			Catch Err Exception
				ErrorProvider1.SetError(txtFastCvrLines, 'Invalid number')
				LeaveSR
			EndTry
			
			FastCvrLines()
		
		When rbSlowCvrStr.Checked
			ErrorProvider1.SetError(txtSlowCvrStr, String.Empty)
			Try
				NumberOf = Convert.ToInt32(txtSlowCvrStr.Text)
			Catch Err Exception
				ErrorProvider1.SetError(txtSlowCvrStr, 'Invalid number')
				LeaveSR
			EndTry

			SlowCvrStr()
		
		When rbSlowCvrLines.Checked
			ErrorProvider1.SetError(txtSlowCvrLines, String.Empty)
			Try
				NumberOf = Convert.ToInt32(txtSlowCvrLines.Text)
			Catch Err Exception
				ErrorProvider1.SetError(txtSlowCvrLines, 'Invalid number')
				LeaveSR
			EndTry
			
			SlowCvrLines()
		
		When rbTopLines.Checked
			ErrorProvider1.SetError(txtTopLines, String.Empty)
			Try
				NumberOf = Convert.ToInt32(txtTopLines.Text)
			Catch Err Exception
				ErrorProvider1.SetError(txtTopLines, 'Invalid number')
				LeaveSR
			EndTry
			
			TopLines()
		
		EndSL
	EndSr 
	
