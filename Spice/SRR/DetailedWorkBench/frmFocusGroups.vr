	// Calculate date from Today by number of weeks backwards
	BegFunc CalcDate Type(*Date) TimFmt(*ISO)
		DclSrParm NumberOfWks Type(*Integer4)
		
		//DclFld Today Type(*Char)  Len(10)
		DclFld Year  Type(*Zoned) Len(4,0)
		DclFld Month Type(*Zoned) Len(2,0)
		DclFld Day   Type(*Zoned) Len(2,0)
		
		//Today = DateTime.Now.ToString()
		//Year  = Convert.ToInt32(Today.SubString(6,4))
		//Month = Convert.ToInt32(Today.SubString(3,2))
		//Day   = Convert.ToInt32(Today.SubString(0,2))
		
		Year  = DateTime.Now.Year
		Month = DateTime.Now.Month
		Day   = DateTime.Now.Day
		
		If Not DSCALEN.IsOpen
			Open DSCALEN
		EndIf
		
		SetLL DSCALEN Key(Year, Month, Day)
		Do FromVal(1) ToVal(NumberOfWks * 7)
			ReadP DSCALEN
			If DSCALEN.IsEof
				Leave
			EndIf
		EndDo
		
		Close DSCALEN
		
		LeaveSR Convert.ToDateTime( CCYY.ToString() + '-' + CCMM.ToString('00') + '-' + CCDD.ToString('00') )
	EndFunc
	
	BegSr cboAllMkts_CheckedChanged Access(*Private) Event(*this.cboAllMkts.CheckedChanged)
		DclSrParm sender *Object
		DclSrParm e System.EventArgs
		
		DclFld X Type(*Integer4)
		
		If cboAllMkts.Checked = *Off
			cboListBoxMkts.Enabled = *True
		Else
			cboListBoxMkts.Enabled = *Off
			
			If cboListBoxMkts.CheckedItems.Count <> *Zero
				Do FromVal(0) toVal(cboListBoxMkts.Items.Count - 1) Index(X)
					cboListBoxMkts.SetItemChecked(X,*True)
				EndDo
			EndIf
		EndIf
	EndSr
	
