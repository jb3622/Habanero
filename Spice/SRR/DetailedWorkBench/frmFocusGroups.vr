	BegSR NewLines
		
		DclFld ReceiptDate Type(*Date) TimFmt(*ISO)
		
		ReceiptDate = CalcDate(NumberOf)
		
		Select
		When rbItemMarket.Checked
			QrySelect = 'IMFRDT *GE %DATE(''' + ReceiptDate + ''') *And IMDPT = ' + wDepartment.toString()
			QryKey    = *Blanks
			Level     = 'IM'
		
		When rbItemGrade.Checked
			QrySelect = 'IGFRDT *GE %DATE(''' + ReceiptDate + ''') *And IGDPT = ' + wDepartment.toString()
			QryKey    = *Blanks
			Level     = 'IG'
					
		When rbItemStore.Checked
			QrySelect = 'ISFRDT *GE %DATE(''' + ReceiptDate + ''') *And ISDPT = ' + wDepartment.toString()
			QryKey    = *Blanks
			Level     = 'IS'
					
		When rbItem.Checked
			QrySelect = 'ITFRDT *GE %DATE(''' + ReceiptDate + ''') *And ITDPT = ' + wDepartment.toString()
			QryKey    = *Blanks
			Level     = 'IT'
		EndSL
		
		InstanceForm()
	EndSR
	
