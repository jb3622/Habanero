				ClassGradeGrid.Rows[SRRgrid7.SelectedRow].Selected = *True
				SelectedClass = Convert.ToInt32(ClassGradeGrid.Item['G7Class',SRRgrid7.SelectedRow].Value)
				SelectedMkt   = ClassGradeGrid.Item['G7Market',SRRgrid7.SelectedRow].Value.ToString().Trim()
				SelectedGrade = ClassGradeGrid.Item['G7Grade',SRRgrid7.SelectedRow].Value.ToString().Trim()
					
				If e.Button = System.Windows.Forms.MouseButtons.Right
					// If Grid is filtered show extra menu items
					If SRRgrid7.isFiltered
						G7mnuSeparator1.Visible = *True
						G7mnuShowAll.Visible = *True
					Else
						G7mnuSeparator1.Visible = *False
						G7mnuShowAll.Visible = *False
					EndIf
					
					G7MenuStrip.Show(ClassGradeGrid, e.X, e.Y)
				EndIf
			EndIf
		EndIf
	EndSR

	BegSR G8_MouseUp Access(*Private) Event(*this.ClassStoreGrid.MouseUp)
		DclSrParm sender *Object
		DclSrParm e System.Windows.Forms.MouseEventArgs
		
		DclFld hti    Type(System.Windows.Forms.DataGridView.HitTestInfo)
		
		If e.Button = System.Windows.Forms.MouseButtons.Right
			If SRRgrid8.SelectedRow > *Zero
				ClassStoreGrid.Rows[SRRgrid8.SelectedRow].Selected = *False
			EndIf

			hti  = ClassStoreGrid.HitTest(e.X, e.Y)
			SRRgrid8.SelectedRow = hti.RowIndex
			
			If hti.Type = DataGridViewHitTestType.Cell
				If SRRgrid8.SelectedRow <> -1
					// Highlight current row
					ClassStoreGrid.Rows[SRRgrid8.SelectedRow].Selected = *True
					SelectedClass = Convert.ToInt32(ClassStoreGrid.Item['G8Class',SRRgrid8.SelectedRow].Value)
					SelectedMkt   = ClassStoreGrid.Item['G8Market',SRRgrid8.SelectedRow].Value.ToString().Trim()
					SelectedGrade = ClassStoreGrid.Item['G8Grade',SRRgrid8.SelectedRow].Value.ToString().Trim()
					SelectedStore = Convert.ToInt32(ClassStoreGrid.Item['G8Store',SRRgrid8.SelectedRow].Value)

					// If Grid is filtered show extra menu items
					If SRRgrid8.isFiltered
						G8MenuStrip.Show(ClassStoreGrid, e.X, e.Y)
					EndIf
				EndIf
			EndIf
		EndIf
	EndSR

