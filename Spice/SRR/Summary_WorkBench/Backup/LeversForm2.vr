	BegSr btnApplyChanges_Click Access(*Private) Event(*this.btnApplyChanges.Click)
		DclSrParm sender *Object
		DclSrParm e System.EventArgs
				
		DclFld MsgBoxCode Type(*Integer2)
		
		Select
		When SRRgrid1 <> *Nothing And SRRgrid1.HasValidationErrors
			MsgBox 'Correct errors before saving model'
			LeaveSR
			
		When SRRgrid2 <> *Nothing And SRRgrid2.HasValidationErrors
			MsgBox 'Correct errors before saving model'
			LeaveSR
			
		When SRRgrid3 <> *Nothing And SRRgrid3.HasValidationErrors
			MsgBox 'Correct errors before saving model'
			LeaveSR
			
		When SRRgrid4 <> *Nothing And SRRgrid4.HasValidationErrors
			MsgBox 'Correct errors before saving model'
			LeaveSR
			
		When SRRgrid5 <> *Nothing And SRRgrid5.HasValidationErrors
			MsgBox 'Correct errors before saving model'
			LeaveSR
			
		When SRRgrid6 <> *Nothing And SRRgrid6.HasValidationErrors
			MsgBox 'Correct errors before saving model'
			LeaveSR
				
		When SRRgrid7 <> *Nothing And SRRgrid7.HasValidationErrors
			MsgBox 'Correct errors before saving model'
			LeaveSR
			
		When SRRgrid8 <> *Nothing And SRRgrid8.HasValidationErrors
			MsgBox 'Correct errors before saving model'
			LeaveSR				
		EndSL
		
		MsgBox 'Do you want to save this model ?' Buttons(*YesNo) Icon(*Question) +
				DftButton(1) RtnCode(MsgBoxCode)
				
		If MsgBoxCode = 6
		
			// Check if fileGroup still locked to user
			If Not DSSRLCKL8.IsOpen
			   Open DSSRLCKL8
			Endif	
       		Chain DSSRLCKL8 Key(12,Department,'S',MemberName)
       		If Not %Found(DSSRLCKL8) Or (%Found(DSSRLCKL8) *And LOUSR <> UserProfile)
			   MsgBox 'File Group is not currently allocated to you. Changes will not be applied'  Icon(*Exclamation)
			   LeaveSR
		    Endif
		
			SetMousePtr *HourGlass
			
			With toolStripStatusLabel2
				.Text = 'Applying changes... Please wait'
				.Visible = *On
			EndWith
			
			With toolStripProgressBar1
				.Minimum = *Zero
				.Maximum = 5
				.Value   = *Zero
				.Visible = *On
			EndWith
			
			DoEvents
			
			// Saving to Model files
			SRRuplift.Save_WorkingModel(MemberName)
			*this.toolStripProgressBar1.Value += 1
			DoEvents
			
			SRRsmooth.Save_WorkingModel(MemberName)
			*this.toolStripProgressBar1.Value += 1
			DoEvents
			
			SRRcutoff.Save_WorkingModel(MemberName)
			*this.toolStripProgressBar1.Value += 1
			DoEvents
			
			SRRalloc.Save_WorkingModel(MemberName)
			*this.toolStripProgressBar1.Value += 1
			DoEvents

			Job    = 'S883BC0001'
			Pgm    = 'DS883AC'
			
			Clear Buffer2
			Fld1b  = MemberName
			Fld2b  = FromMember
			Fld3b  = Department.ToString('000')
			
			Optn1  = Buffer2		// This is part of a data structure
			Optn2  = *Blank
			Submit = 'N'
			
			Command = 'SBMJOB CMD(CALL PGM(DSIPWRAP) PARM(' +                                         +
			    "'" + %Trim(Job) + "'" + ' ' + "'" + %Trim(Pgm) + "'" + ' ' + "'" + %Trim(Optn1) +    + 
			    "'" + ' ' + "'" + %Trim(Optn2) + "'" + ' ' + "'" + %Trim(Submit) +                    +
			    "'" + ' ' + "'" + %Trim(Error) + "'" + ')) JOBQ(QGPL/QTXTSRCH)' 
			
			Length  = %Size(Command) 
			
			Try
				Call Pgm("QCMDEXC") ParmList(CommandEXC) DB( SRR )
			Catch Err Exception
				MsgBox 'Error calling DS883AC, Contact Support' Icon(*Stop)
				LeaveSR
			Endtry
			
			toolStripProgressBar1.Value += 1
			DoEvents

			*this.toolStripProgressBar1.Visible = *Off
			*this.toolStripStatusLabel2.Visible = *Off
			
			// Exit out of this form
			*this.DialogResult = DialogResult.OK
		Else
			LeaveSR
		EndIf
	EndSr
	
	BegSr btnModelRun_Click Access(*Private) Event(*this.btnModelRun.Click)
		DclSrParm sender *Object
		DclSrParm e System.EventArgs
		
		DclFld RtnCode   Type(*Integer2)
		DclFld TabNumber Type(*Integer2)
		
		Select
		When SRRgrid1 <> *Nothing And SRRgrid1.HasValidationErrors
			MsgBox 'Correct errors before running the model'
			LeaveSR
			
		When SRRgrid2 <> *Nothing And SRRgrid2.HasValidationErrors
			MsgBox 'Correct errors before running the model'
			LeaveSR
			
		When SRRgrid3 <> *Nothing And SRRgrid3.HasValidationErrors
			MsgBox 'Correct errors before running the model'
			LeaveSR
			
		When SRRgrid4 <> *Nothing And SRRgrid4.HasValidationErrors
			MsgBox 'Correct errors before running the model'
			LeaveSR
			
		When SRRgrid5 <> *Nothing And SRRgrid5.HasValidationErrors
			MsgBox 'Correct errors before running the model'
			LeaveSR
			
		When SRRgrid6 <> *Nothing And SRRgrid6.HasValidationErrors
			MsgBox 'Correct errors before running the model'
			LeaveSR
				
		When SRRgrid7 <> *Nothing And SRRgrid7.HasValidationErrors
			MsgBox 'Correct errors before running the model'
			LeaveSR
			
		When SRRgrid8 <> *Nothing And SRRgrid8.HasValidationErrors
			MsgBox 'Correct errors before running the model'
			LeaveSR				
		EndSL		
		
		// Confirm action with users
		MsgBox 'Are you sure you want to run a model ?' Buttons(*YesNo) Icon(*Question) +
				DftButton(1) RtnCode(RtnCode)
				
		If RtnCode = 6
		
			// Check if fileGroup still locked to user
			If Not DSSRLCKL8.IsOpen
			   Open DSSRLCKL8
			Endif	
       		Chain DSSRLCKL8 Key(12,Department,'S',MemberName)
       		If Not %Found(DSSRLCKL8) Or (%Found(DSSRLCKL8) *And LOUSR <> UserProfile)
			   MsgBox 'File Group is not currently allocated to you. Changes will not be applied'  Icon(*Exclamation)
			   LeaveSR
		    Endif
		
			SetMousePtr *HourGlass
			
			With toolStripStatusLabel2
				.Text = 'Model Run in Progress... Please wait'
				.Visible = *On
			EndWith
			
			With toolStripProgressBar1
				.Minimum = *Zero
				.Maximum = 15
				.Value   = *Zero
				.Visible = *On
			EndWith
			
			DoEvents
			
			// Save number of open tab because this has to be rebuilt
			TabNumber = TabControl.SelectedIndex
			
			
			// Update the model member with current data
			SRRuplift.Save_WorkingModel(MemberName)
			*this.toolStripProgressBar1.Value += 1
			DoEvents
			
			SRRsmooth.Save_WorkingModel(MemberName)
			*this.toolStripProgressBar1.Value += 1
			DoEvents
			
			SRRcutoff.Save_WorkingModel(MemberName)
			*this.toolStripProgressBar1.Value += 1
			DoEvents
			
			SRRalloc.Save_WorkingModel(MemberName)
			*this.toolStripProgressBar1.Value += 1
			DoEvents
			
			
			// Destroy Detail memory file
			If SRRdetails <> *Nothing
				SRRdetails = *Nothing
			EndIf
			*this.toolStripProgressBar1.Value += 1
			DoEvents
			
			// Copy Item records into current modeling member
			FrmMbr = FromMember
			ToMbr  = MemberName
			Dept   = Department
			
			Try
				Call Pgm("*Libl/DS882XC") ParmList( CpyMbrParms ) DB( SRR )
			Catch Err Exception
				MsgBox 'Unable to copy Item records. Contact Support'
				LeaveSR
			EndTry
			*this.toolStripProgressBar1.Value += 1
			DoEvents
			
			// Set buffer ready for modeling programs
			Buffer = *Blank
			Fld1   = 'M'
			
			If %SubSt(MemberName,1,3) = 'DPT'
				Fld2 = 'W'
			Else
				Fld2 = 'D'
			EndIf
			
			Fld3   = Department.ToString('000')
			Fld4   = FileGroup.ToString('000')
			
			
			//-------------------------------------------------------------
			// Program 1
			//-------------------------------------------------------------
			Job    = 'S882DC0001'
			Pgm    = 'DS882CC'
			Optn1  = Buffer		// This is part of a data structure
			Optn2  = *Blank
			Submit = 'N'
			
			Try
				Call Pgm("*Libl/DSIPWRAP") ParmList( DSIPWRAP ) DB( SRR )
				If Error = 'Y'
					MsgBox 'Error while running DS882CC, contact Support' Icon(*Stop)
					LeaveSR
				EndIf
			Catch Err Exception
				MsgBox 'Error calling DS882CC, contact Support' Icon(*Stop)
				LeaveSR
			EndTry
			*this.toolStripProgressBar1.Value += 1
			DoEvents
			
			//-------------------------------------------------------------
			Job  = 'S882FC0001'
			Pgm  = 'DS882EC'
			
			Try
				Call Pgm("*Libl/DSIPWRAP") ParmList( DSIPWRAP ) DB( SRR )
				If Error = 'Y'
					MsgBox 'Error while running DS882EC, contact Support' Icon(*Stop)
					LeaveSR
				EndIf
			Catch Err Exception
				MsgBox 'Error Calling DS882EC, contact Support' Icon(*Stop)
				LeaveSR
			EndTry
			*this.toolStripProgressBar1.Value += 1
			DoEvents
			
			//-------------------------------------------------------------
			Job  = 'S882HC0001'
			Pgm  = 'DS882GC'
			
			Try
				Call Pgm("*Libl/DSIPWRAP") ParmList( DSIPWRAP ) DB( SRR )
				If Error = 'Y'
					MsgBox 'Error while running DS882GC, contact Support' Icon(*Stop)
					LeaveSR
				EndIf
			Catch Err Exception
				MsgBox 'Error while running DS882GC, contact Support' Icon(*Stop)
				LeaveSR
			EndTry
			*this.toolStripProgressBar1.Value += 1
			DoEvents
			
			// Remove copied in records
			SRRuplift.Rmv_Item_Rec(MemberName)
			*this.toolStripProgressBar1.Value += 1
			DoEvents
			
			SRRsmooth.Rmv_Item_Rec(MemberName)
			*this.toolStripProgressBar1.Value += 1
			DoEvents
			
			SRRcutoff.Rmv_Item_Rec(MemberName)
			*this.toolStripProgressBar1.Value += 1
			DoEvents
			
			SRRalloc.Rmv_Item_Rec(MemberName)
			*this.toolStripProgressBar1.Value += 1
			DoEvents
			
			
			// Create details memory file
			SRRdetails  = *New SRRdetails(SRR, MemberName)
			*this.toolStripProgressBar1.Value += 1
			DoEvents
			
			// Clear the grids, reload the active one
			If SRRgrid1 <> *Nothing
				If TabNumber = Grids.DeptTab
					RemoveHandler SourceObject(DeptGrid) SourceEvent(CellValueChanged) +
						HandlerSR(DeptGrid_CellValueChanged)
						
					SRRgrid1.Fill_Grid(DeptGrid, SRRdetails)
					
					AddHandler SourceObject(DeptGrid) SourceEvent(CellValueChanged) +
						HandlerSR(DeptGrid_CellValueChanged)
				Else
					SRRgrid1.ClearGrid(DeptGrid)
				EndIf
			EndIf
			
			If SRRgrid2 <> *Nothing
				If TabNumber = Grids.DeptMarketTab
					RemoveHandler SourceObject(DeptMarketGrid) SourceEvent(CellValueChanged) +
						HandlerSR(DeptMarketGrid_CellValueChanged)
					
					SRRgrid2.Fill_Grid(DeptMarketGrid, SRRdetails)
					
					AddHandler SourceObject(DeptMarketGrid) SourceEvent(CellValueChanged) +
						HandlerSR(DeptMarketGrid_CellValueChanged)
				Else
					SRRgrid2.ClearGrid(DeptMarketGrid)
				EndIf
			EndIf
			
			If SRRgrid3 <> *Nothing
				If TabNumber = Grids.DeptGradeTab
					RemoveHandler SourceObject(DeptGradeGrid) SourceEvent(CellValueChanged) +
						HandlerSR(DeptGradeGrid_CellValueChanged)
					
					SRRgrid3.Fill_Grid(DeptGradeGrid, SRRdetails)
					
					AddHandler SourceObject(DeptGradeGrid) SourceEvent(CellValueChanged) +
						HandlerSR(DeptGradeGrid_CellValueChanged)
				Else
					SRRgrid3.ClearGrid(DeptGradeGrid)
				EndIf
			EndIf
		
			If SRRgrid4 <> *Nothing
				If TabNumber = Grids.DeptStoreTab
					RemoveHandler SourceObject(DeptStoreGrid) SourceEvent(CellValueChanged) +
						HandlerSR(DeptStoreGrid_CellValueChanged)
					
					SRRgrid4.Fill_Grid(DeptStoreGrid, SRRdetails)
					
					AddHandler SourceObject(DeptStoreGrid) SourceEvent(CellValueChanged) +
						HandlerSR(DeptStoreGrid_CellValueChanged)
				Else	
					SRRgrid4.ClearGrid(DeptStoreGrid)
				EndIf
			EndIf
			
			If SRRgrid5 <> *Nothing
				If TabNumber = Grids.ClassTab
					RemoveHandler SourceObject(ClassGrid) SourceEvent(CellValueChanged) +
						HandlerSR(ClassGrid_CellValueChanged)
					
					SRRgrid5.Fill_Grid(ClassGrid, SRRdetails)
					
					AddHandler SourceObject(ClassGrid) SourceEvent(CellValueChanged) +
						HandlerSR(ClassGrid_CellValueChanged)
				Else
					SRRgrid5.ClearGrid(ClassGrid)
				EndIf
			EndIf
			
			If SRRgrid6 <> *Nothing
				If TabNumber = Grids.ClassMarketTab
					RemoveHandler SourceObject(ClassMarketGrid) SourceEvent(CellValueChanged) +
						HandlerSR(ClassMarketGrid_CellValueChanged)
					
					SRRgrid6.Fill_Grid(ClassMarketGrid, SRRdetails)
					
					AddHandler SourceObject(ClassMarketGrid) SourceEvent(CellValueChanged) +
						HandlerSR(ClassMarketGrid_CellValueChanged)
				Else
					SRRgrid6.ClearGrid(ClassMarketGrid)
				EndIf
			EndIf
		
			If SRRgrid7 <> *Nothing
				If TabNumber = Grids.ClassGradeTab
					RemoveHandler SourceObject(ClassGradeGrid) SourceEvent(CellValueChanged) +
						HandlerSR(ClassGradeGrid_CellValueChanged)
					
					SRRgrid7.Fill_Grid(ClassGradeGrid, SRRdetails)
					
					AddHandler SourceObject(ClassGradeGrid) SourceEvent(CellValueChanged) +
						HandlerSR(ClassGradeGrid_CellValueChanged)
				Else
					SRRgrid7.ClearGrid(ClassGradeGrid)
				EndIf
			EndIf
			
			If SRRgrid8 <> *Nothing
				If TabNumber = Grids.ClassStoreTab
					RemoveHandler SourceObject(ClassStoreGrid) SourceEvent(CellValueChanged) +
						HandlerSR(ClassStoreGrid_CellValueChanged)
					
					SRRgrid8.Fill_Grid(ClassStoreGrid, SRRdetails)
					
					AddHandler SourceObject(ClassStoreGrid) SourceEvent(CellValueChanged) +
						HandlerSR(ClassStoreGrid_CellValueChanged)
				Else
					SRRgrid8.ClearGrid(ClassStoreGrid)
				EndIf
			EndIf
			
			*this.toolStripProgressBar1.Value += 1
			
			DoEvents
			
			SetMousePtr *Dft
			
			MsgBox 'Model Run has completed' Buttons(*Ok) Icon(*Information)
			*this.toolStripProgressBar1.Visible = *Off
			*this.toolStripStatusLabel2.Visible = *Off
		Else
			LeaveSR
		EndIf
	EndSr
	
	// The following "Mouse Up" routine determines if the right mouse button was clicked
	// over a grid cell in the Dept Market Grid, the whole row is then highlighted and
	// the context menu is displayed
	BegSR G2_MouseUp Access(*Private) Event(*this.DeptMarketGrid.MouseUp)
		DclSrParm sender *Object
		DclSrParm e System.Windows.Forms.MouseEventArgs
		
		DclFld hti    Type(System.Windows.Forms.DataGridView.HitTestInfo)
		DclFld Column Type(*Integer2)
		
		If SRRgrid2.SelectedRow > *Zero
			DeptMarketGrid.Rows[SRRgrid2.SelectedRow].Selected = *False
		EndIf
		
		hti    = DeptMarketGrid.HitTest(e.X, e.Y)
		SRRgrid2.SelectedRow = hti.RowIndex
		Column = hti.ColumnIndex
		
		If hti.Type = DataGridViewHitTestType.Cell
			If SRRgrid2.SelectedRow <> -1
				// Highlight current row
				DeptMarketGrid.Rows[SRRgrid2.SelectedRow].Selected = *True
				SelectedMkt = DeptMarketGrid.Item['G2Market',SRRgrid2.SelectedRow].Value.ToString().Trim()
				
				If e.Button = System.Windows.Forms.MouseButtons.Right
					G2MenuStrip.Show(DeptMarketGrid, e.X, e.Y)
				EndIf
			EndIf
		EndIf
	EndSr
	
