		DclSrParm e System.Windows.Forms.DrawItemEventArgs
		
		DclFld G     Type(system.Drawing.Graphics)
		DclFld Pen   Type(system.Drawing.Pen)        New(Color.Black)
		DclFld Font  Type(system.Drawing.Font)       New("Microsoft Sans Serif", 8.25)
		DclFld Brush Type(system.Drawing.SolidBrush) New(Color.Black)
		
