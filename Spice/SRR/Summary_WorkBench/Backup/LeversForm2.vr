		When TabNumber = 4
			G.DrawString("Class", Font, Brush, TextArea4)
		When TabNumber = 5
			G.DrawString("Dept", Font, Brush, TextArea5)
		When TabNumber = 6
			G.DrawString("Dept", Font, Brush, TextArea6)
		When TabNumber = 7
			G.DrawString("Dept", Font, Brush, TextArea7)
