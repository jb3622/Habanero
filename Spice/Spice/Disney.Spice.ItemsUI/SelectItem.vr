			.ValueMember   = "CharCode"
		EndWith

		If @ManagementMode = 'Y'
			With cboDepartments
				.DataSource    = SpiceDS.Tables("Departments")
				.DisplayMember = "cboDepartment"
				.ValueMember   = "Department"
			EndWith
		Else
			cboDepartments.Text    = PassedDept.ToString()
			cboDepartments.Enabled = *False
		EndIf
		
		If @ManagementMode = 'Y'
			With clbClass
				.DataSource    = SpiceDS.Tables("FilteredClasses")
				.DisplayMember = "ClassCodeName"
				.ValueMember   = "ClassCode"
			EndWith
		Else
			With clbClass
				.Enabled       = *True
				.DataSource    = SpiceDS.Tables("DeptClasses")
				.DisplayMember = "ClassCodeName"
				.ValueMember   = "ClassCode"
			EndWith
		Endif	
		
		With clbVendor
			.Enabled       = *True
			.DataSource    = GetDedupedVendors()
			.DisplayMember = "VendorNumberName"
			.ValueMember   = "VendorNumber"
		EndWith
			
		With clbSeason
			.DataSource    = SpiceDS.Tables("Seasons")
			.DisplayMember = "SeasonCodeDescription"
			.ValueMember   = "SeasonCode"
		EndWith

		// Event handlers
		AddHandler SourceObject(cboDepartments) SourceEvent(SelectedValueChanged) HandlerSR(cboDepartments_SelectedValueChanged)
		AddHandler SourceObject(cboFamily)      SourceEvent(SelectedValueChanged) HandlerSR(cboFamily_SelectedValueChanged)
		
		If @ManagementMode = 'Y'
			*this.cboDepartments.Text = "All Authorised Departments"
		Else
			*this.cboDepartments.Text = "Selected Department"
		EndIf
		
		*this.cboFamily.Text = "All Families"
				
