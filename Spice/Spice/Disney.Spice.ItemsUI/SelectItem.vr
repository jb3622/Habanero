	BegSr btnStdFind_Click Access(*Private) Event(*this.btnStdFind.Click)
		DclSrParm sender *Object
		DclSrParm e System.EventArgs
		
		If btnStdFind.Text = "Stop Search"
			bgwSearch.CancelAsync()
			LeaveSR
		EndIf
		
		ErrPro.SetControlError(btnStdFind,String.Empty)
		ErrPro.SetControlError(btnChangeItem,String.Empty)
		ErrPro.SetControlError(btnCopyItem,String.Empty)
		*this.ValidateChildren()

		If ErrPro.ErrorCount <> *Zero
			LeaveSR
		EndIf
		
		Select
		When txtVendStyle.TextLength <> *Zero
		
			btnStdFind.Enabled      = *Off
			btnDspAdvSearch.Enabled = *Off
			SetMousePtr *HourGlass
			
			SearchByVendorStyle()
			
			SetMousePtr *Dft
			btnStdFind.Enabled      = *On
			btnDspAdvSearch.Enabled = *On
			
		When txtUPC.TextLength <> *Zero
		
			btnStdFind.Enabled = *Off
			btnDspAdvSearch.Enabled = *Off
			SetMousePtr *HourGlass
			
			SearchByUPC()
			
			SetMousePtr *Dft
			btnStdFind.Enabled      = *On
			btnDspAdvSearch.Enabled = *On		
		Other
			
			If txtSize.TextLength <> *Zero And txtColour.TextLength = *Zero
				ErrPro.SetControlError(txtColour,'Size is entered, so Colour is required')
			EndIf
			
			If txtColour.TextLength <> *Zero And txtStyle.TextLength = *Zero
				ErrPro.SetControlError(txtStyle,'Size is entered, so Style is required')
			EndIf
			
			If txtStyle.TextLength <> *Zero And txtVendor.TextLength = *Zero
				ErrPro.SetControlError(txtVendor,'Style is entered, so Vendor is required')
			EndIf
			
			If txtVendor.TextLength <> *Zero And txtClass.TextLength = *Zero
				ErrPro.SetControlError(txtClass,'Vendor is entered, so Class is required')
			EndIf
			
			If txtClass.TextLength <> *Zero And txtVendor.TextLength = *Zero
				ErrPro.SetControlError(txtVendor,'Both Class and Vendor are required as a minimum')
			EndIf
			
			If txtClass.TextLength  = *Zero And txtVendor.TextLength = *Zero
				ErrPro.SetControlError(txtClass,'Both Class and Vendor are required as a minimum')
			Endif
			
			If ErrPro.ErrorCount = *Zero
			
				btnStdFind.Text = "Stop Search"
				btnDspAdvSearch.Enabled = *Off
				
				// Determine search level
				Select
				When txtSize.TextLength <> *Zero
					bgwSearch.RunWorkerAsync(5)

				When txtColour.TextLength <> *Zero
					bgwSearch.RunWorkerAsync(4)

				When txtStyle.TextLength <> *Zero
					bgwSearch.RunWorkerAsync(3)

				When txtVendor.TextLength <> *Zero
					bgwSearch.RunWorkerAsync(2)

				EndSL
			EndIf
		EndSL
	EndSr
	
	BegSr SearchByVendorStyle Access(*Private)
				
		dtItems = ItemsBO.SearchByVendorStyle(txtVendStyle.Text)
		With dgvItems
			.AutoGenerateColumns = *False
			.DataSource = dtItems
		EndWith
		
		If dtItems = *Nothing
			ErrPro.SetControlError(*this.btnStdFind,'No Items found, or you are not authorised to the Items Departments')
		ElseIf dtItems.Rows.Count = *Zero
			ErrPro.SetControlError(*this.btnStdFind,'No Items found, or you are not authorised to the Items Departments')
		EndIf
		
		SetMousePtr *Dft
		btnStdFind.Enabled = *True
	EndSr
	
