	/Region Class Validation
	BegSr btnClass_Click Access(*Private) Event(*this.btnClass.Click)
		DclSrParm sender *Object
		DclSrParm e System.EventArgs
		
		DclFld ClassEnquiry Type(Disney.Spice.LookUp.Enquiry)
		
		If PassedDept = *Zero
			SpiceDS = ItemsBO.GetClassTbl()
			If ClassEnquiry = *Nothing
				ClassEnquiry = *New Disney.Spice.LookUp.Enquiry(SpiceDS.Tables("FilteredClasses"),"Classes")
			EndIf
		Else
			SpiceDS = ItemsBO.GetClassTbl(PassedDept)
			If ClassEnquiry = *Nothing
				ClassEnquiry = *New Disney.Spice.LookUp.Enquiry(SpiceDS.Tables("DeptClasses"),"Classes")
			EndIf
		EndIf
		
		ClassEnquiry.ShowGrid()
		If ClassEnquiry.DialogResult = DialogResult.OK
			txtClass.Text = ClassEnquiry.SelectedValue[0]
			ErrPro.SetControlError(txtClass,String.Empty)
		EndIf
	EndSr
	
	BegSr txtClass_Validating Access(*Private) Event(*this.txtClass.Validating)
		DclSrParm sender *Object
		DclSrParm e System.ComponentModel.CancelEventArgs
		
		ErrPro.SetControlError(txtClass,String.Empty)
		If Not String.IsNullOrEmpty(txtClass.Text)
			Try
				SelectedClass = Convert.ToInt16(txtClass.Text)
			Catch Name(Err) Type(System.Exception)
				ErrPro.SetControlError(txtClass,'Invalid number')
				LeaveSR
			EndTry
			If PassedDept = *Zero	
				If Not ItemsBO.GetClass(SelectedClass)
					ErrPro.SetControlError(txtClass,'Class is not defined or you are not authorised to it')
				EndIf
			Else
				If Not ItemsBO.GetClass(SelectedClass,PassedDept)
					ErrPro.SetControlError(txtClass,'Class is not defined or you are not authorised to it')
				EndIf
			EndIf
		EndIf
	EndSr
	
	BegSr txtClass_TextChanged Access(*Private) Event(*this.txtClass.TextChanged)
		DclSrParm sender *Object
		DclSrParm e System.EventArgs
		
//		If txtClass.TextLength = txtClass.MaxLength
//			If String.IsNullOrEmpty(ErrPro.GetError(txtClass))
//				txtVendor.Select()
//			EndIf
//		EndIf
		
		If txtClass.TextLength <> *Zero
			txtUPC.Enabled       = *False
			txtVendStyle.Enabled = *False
		Else
			AreItemFieldsEmpty()
		EndIf
	EndSr
