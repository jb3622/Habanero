	/Region Search background workers
	BegSr bgwSearch_DoWork Access(*Private) Event(*this.bgwSearch.DoWork)
		DclSrParm sender *Object
		DclSrParm e System.ComponentModel.DoWorkEventArgs
		
		DclFld PartsEntered Type(Int16)
		
		PartsEntered = Int16.Parse(e.Argument.ToString())
		
		Select
		When PartsEntered = 2
			dtItems = ItemsBO.SearchByItemNumber(bgwSearch,SelectedClass,SelectedVendor)
		When PartsEntered = 3
			dtItems = ItemsBO.SearchByItemNumber(bgwSearch,SelectedClass,SelectedVendor,SelectedStyle)
		When PartsEntered = 4
			dtItems = ItemsBO.SearchByItemNumber(SelectedClass,SelectedVendor,SelectedStyle,SelectedColour)
		When PartsEntered = 5
			dtItems = ItemsBO.SearchByItemNumber(SelectedClass,SelectedVendor,SelectedStyle,SelectedColour,SelectedSize)
		When PartsEntered = 6
			dtItems = ItemsBO.SearchByItemNumber(SelectedClass,SelectedVendor,SelectedStyle,SelectedColour,SelectedSize)
		EndSL
	EndSr
	
	BegSr bgwSearch_RunWorkerCompleted Access(*Private) Event(*this.bgwSearch.RunWorkerCompleted)
		DclSrParm sender *Object
		DclSrParm e System.ComponentModel.RunWorkerCompletedEventArgs
		
		DclFld str Type(String)
		DclFld i Type(*Integer4) Inz(0)
		DclFld rowsCount  Type(*Integer4) Inz(0)
		DclFld rowRemoved  Type(Boolean) Inz(*True)
		
		If(m_SellingChannel <> *Nothing)
			If (*this.m_SellingChannel <> String.Empty)
				// Filter out search items that do not match the 
				// selling channel 
				DoWhile (rowRemoved = *True)
					rowRemoved = *False
					Foreach Name(row) Collection(dtItems.Rows) type(DataRow)
						Try
							str = row["SellingChannel"] *AS String
							str = str.Trim()
							If(str <> m_SellingChannel)
								dtItems.Rows.Remove(row)
								rowRemoved = *True
								Leave
							EndIf	
						Catch Name(exc) Type(Exception)
							Leave
						EndTry
					EndFor
				EndDo				
			EndIf
		EndIf
		
		With dgvItems
			.AutoGenerateColumns = *False
			.DataSource = dtItems
		EndWith
		
		If dtItems = *Nothing
			ErrPro.SetControlError(*this.btnStdFind,'No Items found, or you are not authorised to the Items Departments')
		ElseIf dtItems.Rows.Count = *Zero
			ErrPro.SetControlError(*this.btnStdFind,'No Items found, or you are not authorised to the Items Departments')
		EndIf
		
		btnStdFind.Text         = "Search"
		btnDspAdvSearch.Enabled = *On
	EndSr
	
