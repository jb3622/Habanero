		Else
			txtClass.Enabled  = *True
			txtVendor.Enabled = *True
			txtStyle.Enabled  = *True
			txtColour.Enabled = *True
			txtSize.Enabled   = *True
			txtVendStyle.Enabled = *True
			btnClass.Enabled  = *True
			btnVendor.Enabled = *True
		EndIf		
		
		If txtUPC.Size = txtUPC.MaximumSize
			btnStdFind.Select()
		EndIf
	EndSr
	
	BegSr txtUPC_Validating Access(*Private) Event(*this.txtUPC.Validating)
		DclSrParm sender *Object
		DclSrParm e System.ComponentModel.CancelEventArgs
		
		DclFld UPC Type(Int64)
	
		If Not String.IsNullOrEmpty(txtUPC.Text)
			Try
				UPC = Convert.ToInt64(txtUPC.Text)
			Catch Name(Err) Type(System.Exception)
				ErrPro.SetControlError(txtUPC,'Invalid number')
				txtUPC.SelectAll()
				LeaveSR
			EndTry
		EndIf
	EndSr
	/EndRegion 
	
	/Region Standard search fields cross validation
	BegSr AreItemFieldsEmpty
		
		If  String.IsNullOrEmpty(txtClass.Text)    +
		And String.IsNullOrEmpty(txtVendor.Text)   +
		And String.IsNullOrEmpty(txtStyle.Text)    +
		And String.IsNullOrEmpty(txtColour.Text)   +
		And String.IsNullOrEmpty(txtSize.Text)
			txtVendStyle.Enabled = *True
			txtUPC.Enabled       = *True
		EndIf 
	EndSr
	/EndRegion
	
