	BegFunc GetSelectedItems Type(DataTable) Access(*Public)
	
		DclFld ReturnTable Type(DataTable)
		DclFld dv          Type(DataView)
		DclFld dr          Type(DataRow)
		
		*this.ShowDialog()
		ReturnTable = ConstructReturnTbl()
		
		If *this.DialogResult = DialogResult.OK
			dv = *New DataView(dtItems)
			dv.RowFilter = "Select = true"
			ForEach SelectedRow Collection(dv) Type(DataRowView)
				dr = ReturnTable.NewRow()
				
				dr.Item('Class')  = SelectedRow.Item('Class')
				dr.Item('Vendor') = SelectedRow.Item('Vendor')
				dr.Item('Style')  = SelectedRow.Item('Style')
				dr.Item('Colour') = SelectedRow.Item('Colour')
				dr.Item('Size')   = SelectedRow.Item('Size')
				
				ReturnTable.Rows.Add(dr)
			EndFor
		EndIf
		
		LeaveSR ReturnTable
	EndFunc
	
	BegFunc ConstructReturnTbl Access(*Private) Type(DataTable)
	
		DclFld Tbl Type(DataTable)
		
		Tbl = *New DataTable()
		Tbl.TableName = "SelectedItems"
		Tbl.Columns.Add(*New DataColumn('Class', *TypeOf(Int16)))
		Tbl.Columns('Class').ReadOnly = *Off
		Tbl.Columns.Add(*New DataColumn('Vendor',*TypeOf(Int32)))
		Tbl.Columns('Vendor').ReadOnly = *Off
		Tbl.Columns.Add(*New DataColumn('Style', *TypeOf(Int16)))
		Tbl.Columns('Style').ReadOnly = *Off
		Tbl.Columns.Add(*New DataColumn('Colour',*TypeOf(Int16)))
		Tbl.Columns('Colour').ReadOnly = *Off
		Tbl.Columns.Add(*New DataColumn('Size',  *TypeOf(Int16)))
		Tbl.Columns('Size').ReadOnly = *Off
		
		LeaveSR Tbl
	EndFunc
