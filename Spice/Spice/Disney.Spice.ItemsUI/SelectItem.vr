	BegSR cboDepartments_SelectedValueChanged Access(*Private)
		DclSrParm sender *Object
		DclSrParm e System.EventArgs
		
		DclFld cv Type(DataView)
		DclFld vv Type(DataView)
		
		If cboDepartments.Text = 'All Authorised Departments'
			With clbClass
				.DataSource    = SpiceDS.Tables("FilteredClasses")
				.DisplayMember = "ClassCodeName"
				.ValueMember   = "ClassCode"
			EndWith
			
			clbVendor.DataSource    = GetDedupedVendors()
			clbVendor.DisplayMember = "VendorNumberName"
			clbVendor.ValueMember   = "VendorNumber"
		Else
			cv = *New DataView(SpiceDS.Tables("FilteredClasses"))
			cv.RowFilter = "Department = " + cboDepartments.Text
			With clbClass
				.DataSource    = cv
				.DisplayMember = "ClassCodeName"
				.ValueMember   = "ClassCode"
			EndWith
			
			vv = *New DataView(SpiceDS.Tables("FilteredVendors"))
			vv.RowFilter = "Department = " + cboDepartments.Text
			clbVendor.DataSource    = vv
			clbVendor.DisplayMember = "VendorNumberName"
			clbVendor.ValueMember   = "VendorNumber"
		EndIf
	EndSr
	
	BegFunc GetDedupedVendors Type(DataTable) Access(*Private)
		
		DclFld   fv   Type(DataTable)
		DclFld   dr   Type(DataRow)
		DclArray Keys Type(DataColumn) Rank(1)

		fv = SpiceDS.Tables("FilteredVendors").Clone()
			
		Keys = *New DataColumn[] {fv.Columns("VendorNumber")}
		fv.PrimaryKey = Keys
			
		ForEach Vendor Collection(SpiceDS.Tables("FilteredVendors").Rows) Type(DataRow)
			dr = fv.Rows.Find(Vendor["VendorNumber"])
			If dr = *Nothing
				fv.ImportRow(Vendor)
			EndIf
		EndFor
		
		LeaveSR fv
	EndFunc
	
