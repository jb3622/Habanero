	BegSR btnChangeItem_Click Access(*Private) Event(*this.btnChangeItem.Click)
		DclSrParm sender *Object
		DclSrParm e System.EventArgs
		
		DclFld SelectedItems Type(DataView)
		DclFld APPCheck      Type(Boolean)
		
		ErrPro.Clear()
		
		SelectedItems = *New DataView(dtItems)
		SelectedItems.RowFilter = "Select = true"
		If SelectedItems.Count = *Zero
			ErrPro.SetControlError(btnChangeItem,'No Items Selected')
			LeaveSR
		EndIf
				
		ForEach ChgItem Collection(SelectedItems) Type(DataRowView)
			If ChgItem["APP"] *As String = 'APP' *OR ChgItem["APP"] *As String = 'BND'
				If ChangeAPP = *Nothing
					
					//Check if APP Master still exists
					APPCheck = ItemsBO.ChkAPPMaster(ChgItem["Class"]  *As Int16,  +
					                                ChgItem["Vendor"] *As Int32,  +
					                                ChgItem["Style"]  *As Int16,  +
					                                ChgItem["Colour"] *As Int16,  +
					                                ChgItem["Size"]   *As Int16)
					
					If APPCheck = *False
						MsgBox Msg("This APP Master has been Removed. Please refresh your selection")
						LeaveSR
					Endif
					
					SetMousePtr Icon(*AppStarting)
					ChangeAPP = *New frmItemAPPChange(PgmDB,       +
					                                  User,        +
					                                  Environment, +
					                                  MDIform,     +
					                                  ChgItem["Class"]  *As Int16,  +
					                                  ChgItem["Vendor"] *As Int32,  +
					                                  ChgItem["Style"]  *As Int16,  +
					                                  ChgItem["Colour"] *As Int16,  +
					                                  ChgItem["Size"]   *As Int16)
				EndIf
				
				ChangeAPP.ShowDialog()
			Else
				//If ChangeItem = *Nothing
					ChangeItem = *New ChangeItem(PgmDB,User,Environment)
				//EndIf
			
				SetMousePtr Icon(*AppStarting)
				ChangeItem.Change(ChgItem["Class"]  *As Int16, +
							      ChgItem["Vendor"] *As Int32, + 
							      ChgItem["Style"]  *As Int16, +
							      ChgItem["Colour"] *As Int16, +
							      ChgItem["Size"]   *As Int16)
			EndIf
		
			ChgItem["Select"] = *False
			
			//This change was put in to prevent multiple selections for change item. 30/03/2010 Joseph Urbina
			Leave
		EndFor
		
		If ChangeItem <> *Nothing
			ChangeItem = *Nothing
		EndIf
		
		If ChangeAPP <> *Nothing
			ChangeAPP = *Nothing
		EndIf
	EndSr
	
