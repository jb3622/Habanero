/region Cell Events test
	BegSr dgvItemAPAdd_CellValidating Access(*Private) Event(*this.dgvItemAPAdd.CellValidating)
		DclSrParm sender *Object
		DclSrParm e System.Windows.Forms.DataGridViewCellValidatingEventArgs
								
		Column     = e.ColumnIndex
		Row        = e.RowIndex
								
		If Row > -1 and Column > 0                                                and +
			Not dgvItemAPAdd.Columns[e.ColumnIndex].Name.Equals("clmVendor")      and +
			Not dgvItemAPAdd.Columns[e.ColumnIndex].Name.Equals("clmDescription") and +
			Not dgvItemAPAdd.Columns[e.ColumnIndex].Name.Equals("clmColourName")  and +
			Not dgvItemAPAdd.Columns[e.ColumnIndex].Name.Equals("clmSizeName")    and +
			Not dgvItemAPAdd.Columns[e.ColumnIndex].Name.Equals("clmUPC") 
						
			If Row < MaxComponents
				If (dgvItemAPAdd.Columns[e.ColumnIndex].Name.Equals("clmClass")) //and +
					//dgvItemAPAdd.Item["clmDescription",Row].Value.ToString() = String.Empty
				
						//Validate Class for Numeric and blank Entry
						If Int16.TryParse(e.FormattedValue.ToString(), *ByRef GridClass) = *False
							dgvItemAPAdd.Rows[Row].ErrorText = "Please enter a valid Class"
							//e.Cancel = *True
						Else
							dgvItemAPAdd.Rows[Row].ErrorText = String.Empty
						Endif
						
						//Check if Class entered belongs to the correct Department
						If dgvItemAPAdd.Rows[Row].ErrorText = String.Empty
							ChkDeptClass = DepartmentClasses.Copy()
							ChkDeptClassDV = *New DataView(ChkDeptClass)
							ChkDeptClassDV.RowFilter = "ClassCode = " + e.FormattedValue.ToString()
				
							If ChkDeptClassDV.Count = *Zero
								dgvItemAPAdd.Rows[Row].ErrorText = "'This Class does not belong to the correct Department"
								//e.Cancel = *True
							Else
								dgvItemAPAdd.Rows[Row].ErrorText = String.Empty
							Endif
						Endif
					
						//Get the row index to initialise grid fields and Lockdown Header
						If dgvItemAPAdd.Item["clmClass", Row].ErrorText = String.Empty
							RowCount = dgvItemAPAdd.RowCount - 1
							LockDownHeader()
						Endif
					Endif
										
					//Validate Style for Numeric and blank Entry
					If dgvItemAPAdd.Columns[e.ColumnIndex].Name.Equals("clmStyle")
						If Int16.TryParse(e.FormattedValue.ToString(), *ByRef GridStyle) = *False
							dgvItemAPAdd.Rows[Row].ErrorText = "Please enter a valid Style"
							//e.Cancel = *True
						Else
							dgvItemAPAdd.Rows[Row].ErrorText = String.Empty
						Endif
					Endif
						
					//Validate Colour for Numeric and blank Entry
					If dgvItemAPAdd.Columns[e.ColumnIndex].Name.Equals("clmColour")
						If Int16.TryParse(e.FormattedValue.ToString(), *ByRef GridColour) = *False
							dgvItemAPAdd.Rows[Row].ErrorText = "Please enter a valid Colour"
							//e.Cancel = *True
						Else
							dgvItemAPAdd.Rows[Row].ErrorText = String.Empty
						Endif
					Endif
						
					//Validate Size for Numeric and blank Entry
					If dgvItemAPAdd.Columns[e.ColumnIndex].Name.Equals("clmSize")
						If Int16.TryParse(e.FormattedValue.ToString(), *ByRef GridSize) = *False
							dgvItemAPAdd.Rows[Row].ErrorText = "Please enter a valid Size"
							AllOk = *False
							//e.Cancel = *True
						Else
							dgvItemAPAdd.Rows[Row].ErrorText = String.Empty
							AllOk = *True
						Endif
					Endif
					
					If AllOk = *True											
						//Instance ItemsBO Class 
						If ItemsBO  = *Nothing
							ItemsBO = *New Items(Pgmdb, User, Environments) 
						Endif
						
						//Check if Item is a valid Item			
						ItemCheck = ItemsBO.GetItem(GridClass, Vendor, GridStyle, GridColour, GridSize, MasterMarket)

						If ItemCheck  = *False
							dgvItemAPAdd.Rows[Row].ErrorText = "Please enter a valid Item"
							AllOk = *False
						Else
							dgvItemAPAdd.Rows[Row].ErrorText = String.Empty
							AllOk = *True
						Endif
											
						//Check if Item exists in all Markets
						If AllOk = *True
							ForEach MarketRow Collection(MarketTblDV) Type(DataRowView)
								Market  = MarketRow.Item["IPMarket"].ToString().Trim()
								ItemCheck = ItemsBO.GetItem(GridClass, Vendor, GridStyle, GridColour, GridSize, Market)
							EndFor
						
							If ItemCheck  = *False
								dgvItemAPAdd.Rows[Row].ErrorText = "This Item does not exist in all Markets. Please create Item before adding"
								AllOk = *False
							Else
								dgvItemAPAdd.Rows[Row].ErrorText = String.Empty
								AllOk = *True
							Endif
						Endif
				
						//Check if Item is an APP Master Item
						If AllOk = *True
							APPMasterItem      = ItemsBO.APP
	   						If APPMasterItem  = "APP"
								dgvItemAPAdd.Rows[Row].ErrorText = "'This Item is an APP Master Item and cannot be added"
								AllOk = *False
							Else
								dgvItemAPAdd.Rows[Row].ErrorText = String.Empty
								AllOk = *True
							Endif
						Endif
												
						//Get Item Details
						//Get Colour Name and Size Name
						If AllOk = *True
							ItemsBO.GetItem(GridClass, Vendor, GridStyle, GridColour, GridSize, MasterMarket)
							ItemsBO.GetColour(GridColour)
							ItemsBO.GetSize(GridSize)
							dgvItemAPAdd.Item["clmColourName",Row].Value  = ItemsBO.ColourName.ToString().Trim()
							dgvItemAPAdd.Item["clmSizeName",Row].Value    = ItemsBO.SizeName.ToString().Trim()
							dgvItemAPAdd.Item["clmDescription",Row].Value = ItemsBO.ItemLongDescription.ToString().Trim()
							dgvItemAPAdd.Item["clmUPC",Row].Value         = ItemsBO.UPC.ToString().Trim()
							dgvItemAPAdd.Item["clmCost",Row].Value        = ItemsBO.ItemCost.ToString().Trim()
							dgvItemAPAdd.Item["clmRetail",Row].Value	  = ItemsBO.ItemRetail.ToString().Trim()
						Endif	
					Endif	
														
					If e.RowIndex < dgvItemAPAdd.RowCount - 1 and +
						dgvItemAPAdd.Columns[e.ColumnIndex].Name.Equals("clmQty")
						//Qty validation
						If Int16.TryParse(e.FormattedValue.ToString(), *ByRef GridQty) = *False
							dgvItemAPAdd.Rows[Row].ErrorText = "Please enter a valid Quantity"
							AllOk = *False
							//e.Cancel = *True
						Else
							dgvItemAPAdd.Rows[Row].ErrorText = String.Empty
							AllOk = *True
						Endif		
																								
						If AllOk = *True	
							dgvItemAPAdd.Rows[Row].ErrorText = String.Empty
							//Enable the Remove Components Button and the OK Button 
							btnRemoveComponent.Enabled = *True
							btnOK.Enabled              = *True
							InzDataGridRow()
						Endif
					Endif
			Else
				If MaxComponents = 1
					MsgBox Msg('Only ' + MaxComponents + ' Component allowed for the APP Type Selected')
				Else
					MsgBox Msg('Only ' + MaxComponents + ' Components allowed for the APP Type Selected')
				Endif	
			Endif
		Endif
	EndSr
/Endregion

EndClass

