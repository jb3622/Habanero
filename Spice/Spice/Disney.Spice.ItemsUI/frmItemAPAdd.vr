			If PackQty < 1 Or PackQty > 9999
				ErrPro.SetControlError(sender *As Control,'Pack Quantity must be between 1 and 9999')
			EndIf
		Else
			ErrPro.SetControlError(sender *As Control,'Please enter a quantity')
		EndIf
