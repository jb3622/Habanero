	BegSr btnRemoveComponent_Click Access(*Private) Event(*this.btnRemoveComponent.Click)
		DclSrParm sender *Object
		DclSrParm e System.EventArgs
		
		
		ForEach dgvRow Collection(dgvItemAPAdd.Rows) Type(DataGridViewRow)
			dclfld dd type(int16)
				
			If dgvRow.Cells["clmSelect"].Value.ToString() = 'True'
				dd = dgvRow.Index
				If dd > 0
					dgvItemAPAdd.Rows.RemoveAt(dd)
				Else
					MsgBox Msg('The last component on the grid cannot be deleted')
				Endif
			Endif
		EndFor
	EndSr
	
