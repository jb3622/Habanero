	/Region Methods	
	BegSr EASApproveReject_Load Access(*Private) Event(*this.Load)
		DclSrParm sender *Object
		DclSrParm e System.EventArgs
		
		If EAS = *Nothing
			EAS = *New Disney.DA.EAS.DSSPARE(PgmDB, DB2Connection, User)
		EndIf
		
		EAS.GetApprovalDetails(RequestID)
		
		Class      = Convert.ToInt16(%Subst(EAS.RequestDetails, 9, 4))
		Vendor     = Convert.ToInt32(%Subst(EAS.RequestDetails, 14, 5))
		Style      = Convert.ToInt16(%Subst(EAS.RequestDetails, 20, 4))
		Colour     = Convert.ToInt16(%Subst(EAS.RequestDetails, 25, 3))
		Size       = Convert.ToInt16(%Subst(EAS.RequestDetails, 29, 4))
		ChgDtlFlag = %Subst(EAS.RequestDetails, 34, 1)
						
		If ItemsBO = *Nothing
			ItemsBO = *New Disney.Spice.ItemsBO.Items(PgmDB,User,Environment)
		EndIf
		
		If MarketList = *Nothing
			MarketList = *New Disney.DA.IP400.SpiceItem(PgmDB,User)
		EndIf
		
		If AllMarkets = *Nothing
			AllMarkets = *New Disney.DA.IP400.Markets(Environment)
		EndIf
		
		// Retrieve Item details and populate textboxes etc		
		ItemStyle  = ItemsBO.GetItemStyle(Class,Vendor,Style,Colour)
			   
	   //MarketTable Contains all Markets where this Item exists
	   MarketTable = MarketList.GetMarketList(Class,Vendor,Style,Colour,Size) 
	   Markets     = Allmarkets.GetMarketsTbl()
	   
	   MarketString = *New StringBuilder()
	   MarketTblDv = *New DataView(MarketTable)
	   MarketsDv   = *New DataView(Markets)
	   MarketsDv.RowFilter = "Domain  = '" + Domain + "' And Status = 1"  
	   
	   ForEach ALLMarket Collection(MarketsDv)	Type(DataRowView)
			MarketTblDv.RowFilter = "Market = '" + AllMarket.Item["IPMarket"].ToString() + "'"
			If MarketTblDv.Count > 0
				MarketString.Append(ALLMarket.Item["IPMarket"].ToString())
				MarketString.Append(",")
			Endif
		EndFor
	  	   
	   If MarketString.Length > *Zero
			MarketString.Remove(MarketString.Length-1,1)
		EndIf
			      
       lblMarkets.Text = MarketString.ToString()
		
		txtClass.Text  = ItemStyle.Class.ToString('0000')
		ItemsBO.GetClass(ItemStyle.Class)
		lblClassDesc.Text = ItemsBO.ClassName
		
		txtVendor.Text = ItemStyle.Vendor.ToString('00000')
		ItemsBO.GetVendor(ItemStyle.Vendor)
		lblVendorName.Text = ItemsBO.VendorName
		
		txtStyle.Text  = ItemStyle.Style.ToString('0000')
		
		txtColour.Text = ItemStyle.Colour.ToString('000')
		ItemsBO.GetColour(ItemStyle.Colour)
		lblColourDesc.Text = ItemsBO.ColourName
		
		txtSize.Text = Size.ToString('0000')
		ItemsBO.GetSize(Size)
		lblSizeDescription.Text = ItemsBO.SizeName
		
		txtSubClass.Text = ItemStyle.SubClass
		ItemsBO.GetSubClass(ItemStyle.SubClass,ItemStyle.Class)
		lblSubClassDesc.Text = ItemsBO.SubClassName
		If ItemStyle.PendingSubClass <> String.Empty
			lblPendSubClass.Text = "(" + ItemStyle.PendingSubClass + ")"
		Else
			lblPendSubClass.Text = String.Empty
		EndIf
		
		txtLongDesc.Text    = ItemStyle.LongDescription
		txtLocalDescription.Text = ItemStyle.LocalDescription
		txtShortDesc.Text   = ItemStyle.ShortDescription
		txtVendorStyle.Text = ItemStyle.VendorStyle
						
		// Licence code
		ItemsBO.GetLicenceTbl()
		cboLicence.DataSource = ItemsBO.SpiceDataSet.Tables("FilteredLicence")
		cboLicence.DisplayMember = "CodeDescription"
		cboLicence.ValueMember   = "Code"
		If ItemStyle.LicenceCode <> String.Empty
			cboLicence.SelectedValue = ItemStyle.LicenceCode
		EndIf
		
		// Composition Code
		txtCompoCode.Text = ItemStyle.CompositionCode
		ItemsBO.GetCompositionCode(ItemStyle.CompositionCode)
		lblCompoCodeDesc.Text = ItemsBO.CompositionDescription
		
		// Commodity Code
		txtCommodity.Text = ItemStyle.CommodityCode
		ItemsBO.GetCommodityCode(ItemStyle.CommodityCode)
		lblCommodityDesc.Text = ItemsBO.CommodityDescription
		
		// Country of Origin
		txtOrigin.Text = ItemStyle.CountryCode
		ItemsBO.GetCountry(ItemStyle.CountryCode)
		lblOrigin.Text = ItemsBO.CountryName
		
		// Character code
		txtChar.Text = ItemStyle.CharacterCode
		ItemsBO.GetChar(ItemStyle.CharacterCode)
		lblCharName.Text = ItemsBO.CharacterName
		If ItemStyle.PendingCharCode <> String.Empty
			lblPendChar.Text = "(" + ItemStyle.PendingCharCode + ")"
		Else
			lblPendChar.Text = String.Empty
		EndIf
		
		// Season Code
		txtSeason.Text = ItemStyle.SeasonCode
		ItemsBO.GetSeason(ItemStyle.SeasonCode)
		lblSeason.Text = ItemsBO.SeasonDesc
		
		// Ticket Type
		ItemsBO.GetTicketTypeTbl()
		cboTicketType.DataSource = ItemsBO.SpiceDataSet.Tables("TicketType")
		cboTicketType.DisplayMember = "CodeDescription"
		cboTicketType.ValueMember   = "TicketTypeCode"
		If ItemStyle.TicketType <> String.Empty
			cboTicketType.SelectedValue = ItemStyle.TicketType
		EndIf
		
		// Target Age Group
		ItemsBO.GetAgeGroupTbl()
		cboAgeGroup.DataSource = ItemsBO.SpiceDataSet.Tables("AgeGroups")
		cboAgeGroup.DisplayMember = "GroupDescription"
		cboAgeGroup.ValueMember   = "AgeGroupID"
		If ItemStyle.AgeGroup <> String.Empty
			cboAgeGroup.SelectedValue = ItemStyle.AgeGroup
		EndIf
		
		// Promo Code
		txtPromo.Text = ItemStyle.PromotionCode
		ItemsBO.GetPromotion(ItemStyle.PromotionCode)
		lblPromoDesc.Text = ItemsBO.PromotionDescription
		If ItemStyle.PendingPromoCode <> String.Empty
			lblPendPromo.Text = "(" + ItemStyle.PendingPromoCode + ")"
		Else
			lblPendPromo.Text = String.Empty
		EndIf
		
		// Radio buttons
		If ItemStyle.ImpDomInd.Equals('I')
			rbImport.Checked = *true
		Else
			rbDomestic.Checked = *true
		EndIf
		
		Select
		When ItemStyle.GenderCode.Equals('U')
			rbGenderUnisex.Checked = *true
		When ItemStyle.GenderCode.Equals('M')
			rbGenderMale.Checked = *true
		Other
			rbGenderFemale.Checked = *true
		EndSL
		
		If ItemStyle.LocationInd.Equals('N')
			rbLocationNo.Checked = *true
		Else
			rbLocationYes.Checked = *true
		EndIf
		
		If ItemStyle.OnlineInd.Equals("N")
			rbOnlineNo.Checked = *true
		Else
			rbOnlineYes.Checked = *true
		EndIf
				
		//Pending Case Pack Details
		DclFld CasePackDescription Type(String)
		
		CasePack = *New Disney.Spice.ItemsBO.CasePack(PgmDB, Environment)
		ItemsBO.GetPendingCasePackDetails(Class, Vendor, Style, Colour, Size)
		CasePackDescription  = CasePack.GetPackDescription(ItemsBO.PendingCasePackType)
		txtCasePackType.Text = ItemsBO.PendingCasePackType + ' ' + '-' + ' '+ CasePackDescription
		txtCasePackQty.Text  = ItemsBO.PendingCasePackQty
		txtDistroQty.Text    = ItemsBO.PendingDistroInnerQty
		txtWeight.Text       = ItemsBO.PendingWeight
		txtHeight.Text       = ItemsBO.PendingHeight
		txtLength.Text       = ItemsBO.PendingLength
		txtWidth.Text        = ItemsBO.PendingWidth 
		
		If ChgDtlFlag = 'C'
			grpBoxC.Visible = *True
			grpBoxD.Visible = *False
		Else
			grpBoxD.Visible = *True
			grpBoxC.Visible = *False
		Endif
	EndSr
	/EndRegion	

	/Region Buttons	
	BegSr btnHistory_Click Access(*Private) Event(*this.btnHistory.Click)
		DclSrParm sender *Object
		DclSrParm e System.EventArgs
		
		DclFld History Type(Disney.Spice.LookUp.History)
		DclFld ItemNo  Type(StringBuilder)
		
		ItemNo = *New StringBuilder()
			ItemNo.Append(Class.ToString('0000'))
			ItemNo.Append(Vendor.ToString('00000'))
			ItemNo.Append(Style.ToString('0000'))
			ItemNo.Append(Colour.ToString('000'))
			ItemNo.Append(Size.ToString('0000'))
		
			History = *New Disney.Spice.LookUp.History(PgmDB,'ITEM',ItemNo.ToString())
			History.ShowDialog()
			History = *Nothing
	EndSr
	
	BegSr btnEAS_Click Access(*Private) Event(*this.btnEAS.Click)
		DclSrParm sender *Object
		DclSrParm e System.EventArgs
		
		DclFld frmEASRequest Type(Disney.Spice.EASUI.frmEASRequest)
		
		frmEASRequest = *New frmEASRequest(Pgmdb, DB2Connection, User, Environment, MDIParent, RequestID)
		frmEASRequest.ShowDialog()
		frmEASRequest = *Nothing
	EndSr
	
	BegSr btnApprove_Click Access(*Private) Event(*this.btnApprove.Click)
		DclSrParm sender *Object
		DclSrParm e System.EventArgs
		
		DclFld WriteToiSeriesDTAQ Type(WriteToiSeriesDTAQ)
		DclFld Title              Type(String)
		DclFld Msg                Type(String)
		DclFld Response           Type(Int16)
			
		Title    = "Spice - EAS - Confirm Request Approval"
		Msg      = "Please Confirm you are Approving this Request"
		
		 MsgBox(Msg) Buttons(*OKcancel) ICON(*Question) DFTBUTTON(1) RTNCODE(Response)
		
		If Response = 1
			WriteToiSeriesDTAQ = *New WriteToiSeriesDTAQ(PgmDB, User)
			WriteToiSeriesDTAQ.WritePOtoDtaQ("PRCUPDREQ", RequestID, "A", " ")
			Unload *This
		Else
			MsgBox Msg("You cancelled the Approve request. This request will not be processed.")
		Endif
		
	EndSr
	
	BegSr btnReject_Click Access(*Private) Event(*this.btnReject.Click)
		DclSrParm sender *Object
		DclSrParm e System.EventArgs
				
		DclFld RejectReasonForm   Type(RejectReasonForm)
		DclFld WriteToiSeriesDTAQ Type(WriteToiSeriesDTAQ)
		DclFld ReasonCode         Type(String)
		
		RejectReasonForm   = *New RejectReasonForm()
		WriteToiSeriesDTAQ = *New WriteToiSeriesDTAQ(PgmDB, User)
				
		ReasonCode = RejectReasonForm.GetReason()
		
		If Not String.IsNullOrEmpty(ReasonCode)
			WriteToiSeriesDTAQ.WritePOtoDtaQ("PRCUPDREQ", RequestID, "R", ReasonCode)
			Unload *This
		Endif
	EndSr
		
	BegSr btnHelp_Click Access(*Private) Event(*this.btnHelp.Click)
		DclSrParm sender *Object
		DclSrParm e System.EventArgs
			
		OsExec CmdLine("iexplore http://teamshareemea.emea.wdpr.disney.com/dcpsites/spice/SpiceUserManual/Pages/default.aspx")
	EndSr
	
	BegSr btnCancel_Click Access(*Private) Event(*this.btnCancel.Click)
		DclSrParm sender *Object
		DclSrParm e System.EventArgs
		
		DclFld ReturnCode Type(Int16)
		MsgBox Msg("Are you sure you want to cancel this Approve/Reject Request") Title("Spice - EAS - Approve/Reject Request") +
		Buttons(*YesNo) RtnCode(ReturnCode) Icon(*Question) DftButton(1) 
		
		If ReturnCode = 7
			*This.DialogResult = DialogResult.None
		Endif
	EndSr
	/EndRegion
EndClass