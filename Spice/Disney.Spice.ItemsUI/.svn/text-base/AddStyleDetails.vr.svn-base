	BegSr btnNext_Click Access(*Private) Event(*this.btnNext.Click)
		DclSrParm sender *Object
		DclSrParm e System.EventArgs
		
		DclFld dv Type(DataView)
			
		Try
			*this.ValidateChildren()
			
			// Check for any zero values entered into Retail price grid
			ErrPro.SetControlError(dgvPriceMatrix,String.Empty)
			dv = *New DataView(ItemsBO.SpiceDataSet.Tables("RetailPrice"))
			dv.RowFilter = "Retail = 0"
			If dv.Count <> *Zero
				ErrPro.SetControlError(dgvPriceMatrix,"One or more Retail prices are Zero")
			EndIf
			
			// Do existance check where Style has been entered
			If txtStyle.Text <> String.Empty And ErrPro.GetError(txtStyle) = String.Empty
				If  ErrPro.GetError(txtClass)     = String.Empty +
				And ErrPro.GetError(txtVendor)    = String.Empty +
				And ErrPro.GetError(txtColour)    = String.Empty +
						
					If (Not ItemsBO.ChkItemStyleColourUnique())
						ErrPro.SetControlError(txtClass, 'This item already exists')
						ErrPro.SetControlError(txtVendor,'This item already exists')
						ErrPro.SetControlError(txtStyle, 'This item already exists')
						ErrPro.SetControlError(txtColour,'This item already exists')
					EndIf
				EndIf
			EndIf
			
			If ErrPro.ErrorCount = *Zero
			
				dv = *New DataView(ItemsBO.SpiceDataSet.Tables("DomainMarkets"))
				dv.RowFilter = "Select = true"
				ForEach SelectedMkt Collection(dv) Type(DataRowView)
					SelectedMkt["VATcode"]  = ItemsBO.GetVATcode(SelectedMkt["IPMarket"] *As String, ItemsBO.Class)
					SelectedMkt["Exchange"] = ItemsBO.GetExchangeRate(SelectedMkt["IPCurrencyCode"] *As String)
				EndFor
			
				ItemsBO.LicenceCode = cboLicence.Text.Substring(0,3)
				ItemsBO.TicketType  = cboTicketType.Text.Substring(0,1)
				ItemsBO.AgeGroupID  = cboAgeGroup.Text.Substring(0,5)
				ItemsBO.PromotionCode =	txtPromotion.Text
				
				Select
				When rbGenderUnisex.Checked
					ItemsBO.GenderCode = 'U'
				When rbGenderMale.Checked
					ItemsBO.GenderCode = 'M'
				Other
					ItemsBO.GenderCode = 'F'
				EndSL
				
				If rbOnlineNo.Checked
					ItemsBO.OnlineInd = 'N'
				Else
					ItemsBO.OnlineInd = 'Y'
				EndIf
				
				If rbLocationNo.Checked
					ItemsBO.LocationInd = 'N'
				Else
					ItemsBO.LocationInd = 'Y'
				EndIf
				
				If rbImport.Checked
					ItemsBO.ImportDomesticInd = 'I'
				Else
					ItemsBO.ImportDomesticInd = 'D'
				EndIf
				
				If ItemsBO.SpiceDataSet.Tables.Contains("CopyItemSizes")
					ForEach SizeRow Collection(ItemsBO.SpiceDataSet.Tables["CopyItemSizes"].Rows) Type(DataRow)
						If SizeRow["PackQty"] *As Int32 = ItemsBO.CaseQuantity
							SizeRow["PackQty"] = CaseQty
						EndIf
						
						If SizeRow["DistroQty"] *As Int32 = ItemsBO.DistroQuantity
							SizeRow["DistroQty"] = DistroQty
						EndIf
						
						If SizeRow["PackWeight"] *As Decimal = ItemsBO.PackWeight
							SizeRow["PackWeight"] = PackWeight
						EndIf
						
						If SizeRow["PackHeight"] *As Decimal = ItemsBO.PackHeight
							SizeRow["PackHeight"] = PackHeight
						EndIf
						
						If SizeRow["PackLength"] *As Decimal = ItemsBO.PackLength
							SizeRow["PackLength"] = PackLength
						EndIf
						
						If SizeRow["PackWidth"] *As Decimal = ItemsBO.PackWidth
							SizeRow["PackWidth"] = PackWidth
						EndIf
					EndFor
					
					ItemsBO.SpiceDataSet.Tables["CopyItemSizes"].AcceptChanges()
				EndIf
				
				ItemsBO.CaseQuantity   = CaseQty
				ItemsBO.DistroQuantity = DistroQty
				ItemsBO.PackWeight     = PackWeight
				ItemsBO.PackHeight     = PackHeight
				ItemsBO.PackLength     = PackLength
				ItemsBO.PackWidth      = PackWidth
				ItemsBO.PackCode       = PackCode
				ItemsBO.PackOverrideable = PackOverrideable
				If(cboSellingChannel.SelectedIndex = -1)
					errorProvider1.SetError(cboSellingChannel, 'Select a Selling Channel')
					LeaveSR
				Else
					ItemsBO.SellingChannel = *this.cboSellingChannel.SelectedValue.ToString()
				EndIf
				
				*this.DialogResult = DialogResult.OK
			EndIf
		Catch Name(exc) Type(Exception)
			//MsgBox TITLE('Error') Msg(exc.Message)  ICON(*Stop)
		EndTry	
			
	EndSr
	/EndRegion
	
