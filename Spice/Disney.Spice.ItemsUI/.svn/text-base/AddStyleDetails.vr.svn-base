	BegSR RepopulateForm Access(*Private)
		Try 
			If (ItemsBO.Class <> 0)
				txtClass.Text  = ItemsBO.Class.ToString()  
				  ItemsBO.GetClass(ItemsBO.Class)
				  lblClassDesc.Text = ItemsBO.ClassName   
			EndIf
		
			If (ItemsBO.VendorCode <> 0)
				txtVendor.Text = ItemsBO.VendorCode.ToString()
			EndIf
			
			If (ItemsBO.Style <> 0)
				txtStyle.Text  = ItemsBO.Style.ToString()
			EndIf		
			
			If (ItemsBO.Colour <> 0)
				txtColour.Text = ItemsBO.Colour.ToString()
			EndIf	  

			txtSubClass.Text = ItemsBO.SubClass    
			txtLongDesc.Text    = ItemsBO.LongDescription
			txtShortDesc.Text   = ItemsBO.ShortDescription
			txtLocalDescription.Text = ItemsBO.LocalDescription
			txtVendorStyle.Text = ItemsBO.VendorStyle 
			txtCompoCode.Text = ItemsBO.CompositionCode
			txtCommodity.Text = ItemsBO.CommodityCode
			txtOrigin.Text = ItemsBO.CountryCode
			txtChar.Text = ItemsBO.CharacterCode
			lblCharName.Text = ItemsBO.CharacterName
			txtSeason.Text = ItemsBO.SeasonCode
			txtPromotion.Text = ItemsBO.PromotionCode
			
			Select
			When ItemsBO.GenderCode.Equals('U')
			rbGenderUnisex.Checked = *true
			When ItemsBO.GenderCode.Equals('M')
			rbGenderMale.Checked = *true
			Other
			rbGenderFemale.Checked = *true
			EndSL

			If ItemsBO.LocationInd.Equals('N')
			rbLocationNo.Checked = *true
			Else
			rbLocationYes.Checked = *true
			EndIf

			If ItemsBO.OnlineInd.Equals("N")
			rbOnlineNo.Checked = *true
			Else
			rbOnlineYes.Checked = *true
			EndIf

			// Combo boxes
			If ItemsBO.LicenceDescription <> String.Empty
			cboLicence.SelectedValue = ItemsBO.LicenceDescription
			EndIf  
			If ItemsBO.TicketTypeDescription <> String.Empty
			cboTicketType.SelectedValue = ItemsBO.TicketTypeDescription
			EndIf
			If ItemsBO.AgeGroupDescription <> String.Empty
			cboAgeGroup.SelectedValue = ItemsBO.AgeGroupDescription //pItemStyle.AgeGroup
			EndIf
		
			If (ItemsBO.CaseQuantity <> 0)
				txtPackQty.Text = ItemsBO.CaseQuantity.ToString()
			EndIf
			
			If (ItemsBO.DistroQuantity <> 0)
				txtDistroQty.Text = ItemsBO.DistroQuantity.ToString()
			EndIf
		
			If (ItemsBO.StyleCost <> 0)
				txtStyleCost.Text = ItemsBO.StyleCost.ToString()
			EndIf
			
			If (ItemsBO.StyleRetail <> 0)
				txtStyleRetail.Text = ItemsBO.StyleRetail.ToString()
			EndIf		
		
			ItemsBO.GenerateRetailPriceTbl(ItemsBO.StyleRetail,*False,*False)
			dgvPriceMatrix.DataSource = ItemsBO.SpiceDataSet.Tables["RetailPrice"]
	
		Catch Name(exc) Type(Exception)
			//MsgBox TITLE('Error') Msg(exc.Message)  ICON(*Stop)
		EndTry
	EndSR 
	
	
	BegSR AddItem_Load Access(*Private) Event(*this.Load)
		DclSrParm sender *Object
		DclSrParm e System.EventArgs
		
		m_formLoaded = *False
		
		DclFld dt Type(DataTable)
		
		Try
			RepopulateForm()
		Catch Name(exc) Type(Exception)
			MsgBox TITLE('Error - RepopulateForm') Msg(exc.Message)  ICON(*Stop)
		EndTry	
		
		Try
			// Determine the Continent - In Europe the Style cannot be entered
			//If Environment.Domain          = 'EMEA'			
			If (DataCache.IsStyleNumberInputRequired = *False)
				txtStyle.Enabled           = *False
				txtCompoCode.Enabled       = *True
				txtCommodity.Enabled       = *True
				btnCompoCodelookUp.Enabled = *True
				btnCommodityLookUp.Enabled = *True 
			Else
				txtStyle.Enabled           = *True
				txtCompoCode.Text          = "*"
				txtCommodity.Text          = "*"
				txtCompoCode.Enabled       = *False
				txtCommodity.Enabled       = *False
				btnCompoCodelookUp.Enabled = *False
				btnCommodityLookUp.Enabled = *False
			EndIf
			
			lblMarketList.Text = ItemsBO.SelectedMarkets
			lblVendorCcy.Text  = ItemsBO.BaseCurrency
			lblRetailCcy.Text  = ItemsBO.BaseCurrency

			// Licence Combobox (developed by)
			ItemsBO.GetLicenceTbl()
			cboLicence.DataSource    = ItemsBO.SpiceDataSet.Tables("FilteredLicence")
			cboLicence.DisplayMember = "CodeDescription"
			AddHandler SourceObject(cboLicence) HandlerSR(cboLicence_TextChanged) +
				SourceEvent(TextChanged)
			
			// Ticket Type
			ItemsBO.GetTicketTypeTbl()
			cboTicketType.DataSource = ItemsBO.SpiceDataSet.Tables("TicketType")
			cboTicketType.DisplayMember = "CodeDescription"
			AddHandler SourceObject(cboTicketType) HandlerSR(cboTicketType_TextChanged) +
				SourceEvent(TextChanged)
			
			// Selling Channel
			ItemsBO.GetSellingChannelTbl()
			cboSellingChannel.DataSource = ItemsBO.SpiceDataSet.Tables("SellingChannelTable")
			cboSellingChannel.DisplayMember = "CodeAndDescription"
			cboSellingChannel.ValueMember   = "SellingChannelCode"		
			If (ItemsBO.SellingChannel = *Nothing *Or ItemsBO.SellingChannel = String.Empty)
				cboSellingChannel.SelectedIndex = -1
				dt = cboSellingChannel.DataSource *AS DataTable			
				DclFld x *Integer2
				For Index(x = 0) to(dt.Rows.Count - 1)
					If (dt.Rows[x]["ItemsDefault"].ToString() = "Y")
						cboSellingChannel.SelectedValue = dt.Rows[x]["SellingChannelCode"]
					EndIf
				EndFor				
			Else
				cboSellingChannel.SelectedValue = ItemsBO.SellingChannel
			EndIf
			
			// Age Group Combobox
			ItemsBO.GetAgeGroupTbl()
			cboAgeGroup.DataSource    = ItemsBO.SpiceDataSet.Tables("AgeGroups")
			cboAgeGroup.DisplayMember = "GroupDescription"
			cboAgeGroup.ValueMember   = "AgeGroupID"
			Addhandler SourceObject(cboAgeGroup) HandlerSR(cboAgeGroup_TextChanged) +
				 SourceEvent(TextChanged)
		Catch Name(exc) Type(Exception)
			//MsgBox TITLE('Error') Msg(exc.Message)  ICON(*Stop)
		EndTry	
				
		m_formLoaded = *True
	EndSR

	BegSR AddItem_FormClosing Access(*Private) Event(*this.FormClosing)
		DclSrParm sender Type(*Object)
		DclSrParm e Type(System.Windows.Forms.FormClosingEventArgs)
        
		DclFld ReturnCode Type(Int16)
		
		If *this.DialogResult = DialogResult.Cancel
			MsgBox 'Are you sure you want to Cancel?' RtnCode(ReturnCode) Buttons(*YesNo) Title("Cancel ?") Icon(*Question)
			If ReturnCode = 6
				*this.DialogResult = DialogResult.Abort
			Else
				e.Cancel = *true
			EndIf
		EndIf
        
        //Settings.Default.Save()
	EndSR 
