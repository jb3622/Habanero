	/Region UPC
	BegSr txtUPC_validating Access(*Private) Event(*this.txtUPC.Validating)
		DclSrParm sender *Object
		DclSrParm e System.ComponentModel.CancelEventArgs
		
		DclFld UPC           Type(Int64)
		DclFld UPCCheckDigit Type(Boolean)
				
		ErrPro.SetControlError(sender *As Control,String.Empty)
		
		Try
			If Not String.IsNullOrEmpty(txtUPC.Text)
				Try
					UPC = Convert.ToInt64(txtUPC.Text)
				Catch Name(Err) Type(System.Exception)
					ErrPro.SetControlError(txtUPC,'Invalid number')
					LeaveSR
				EndTry
				
				//Calculate UPC Checkdigit
				UPCCheckDigit =  ItemsBO.ChkUPCcheckDigit(UPC)
				If	UPCcheckDigit = *False
					ErrPro.SetControlError(txtUPC,'UPC CheckDigit not valid, Please correct.')
					LeaveSR
				EndIf
				
				// Check that the UPC is unique on IPITHDR DSSPIIA for Vendor and Disney UPC
				If ItemsBO.ChkUPCunique(UPC)
					ItemsBO.UPC = UPC
				Else
					ErrPro.SetControlError(txtUPC,'UPC not unique, enter a different UPC for this new style')
					LeaveSR
				EndIf
				
				If ItemsBO.ChkVendorUPCunique(UPC)
					ItemsBO.UPC = UPC
				Else
					ErrPro.SetControlError(txtUPC,'UPC not unique, enter a different UPC for this new style')
					LeaveSR
				EndIf
				
				If ItemsBO.ChkDisneyUPCunique(UPC)
					ItemsBO.UPC = UPC
				Else
					ErrPro.SetControlError(txtUPC,'UPC not unique, enter a different UPC for this new style')
					LeaveSR
				EndIf
			EndIf
		Catch Name(exc) Type(Exception)
			//MsgBox TITLE('Error') Msg(exc.Message)  ICON(*Stop)
		EndTry

	EndSr
	
