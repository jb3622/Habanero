	/Region Constructor and Entry Code
	BegConstructor Access(*Public)
		DclSrParm PgmDB   Type(ASNA.VisualRPG.Runtime.Database)
		DclSrParm User    Type(Disney.Menu.Users)
		DclSrParm Environment Type(Disney.Menu.Environments)
		DclSrParm ItemsBO Type(Disney.Spice.ItemsBO.Items)

		InitializeComponent()
		
		ErrPro = *New Disney.Spice.ErrorProvider.ErrorProviderWithCount(*this.components)
		ErrPro.BlinkStyle = System.Windows.Forms.ErrorBlinkStyle.NeverBlink
		ErrPro.ContainerControl = *this		

		Try
			*this.PgmDB = PgmDB
			*this.User  = User
			*this.Environment = Environment
			*this.ItemsBO = ItemsBO
			
			//ResetScreenFields()
			*this.lblClassDesc.Text     = String.Empty
			*this.lblVendorDesc.Text    = String.Empty
			*this.lblColourDesc.Text    = String.Empty
			*this.lblSubClassDesc.Text  = String.Empty
			
			*this.lblCharName.Text      = String.Empty
			*this.lblCommodityDesc.Text = String.Empty
			*this.lblCompoCodeDesc.Text = String.Empty
			*this.lblSeason.Text        = String.Empty
			*this.lblPromotionDesc.Text = String.Empty
			*this.lblOrigin.Text        = String.Empty
			
			CasePack = *New Disney.Spice.ItemsBO.CasePack(PgmDB, Environment)
			
			cboCasePackType.DataSource    = CasePack.GetCasePackTbl()
			cboCasePackType.DisplayMember = "PackCodeDescription"
			cboCasePackType.ValueMember   = "CasePackCode"
			
			cboCasePackType.Text = String.Empty
			

			
			If(DataCache.LocalLanguage = 'English')
				//txtLocalDescription.Visible = *False
				//lblLocalDescription.Visible = *False
				lblLocalDescription.Enabled = *False
				txtLocalDescription.Enabled = *False	
				lblLocalDescription.Text = "Description (Other)"
			Else
				//txtLocalDescription.Visible = *True
				//lblLocalDescription.Visible = *True
				lblLocalDescription.Enabled = *True
				txtLocalDescription.Enabled = *True			
				lblLocalDescription.Text = "Description (" + DataCache.LocalLanguage + ")"
			EndIf
			
		Catch Name(exc) Type(Exception)
			//MsgBox TITLE('Error') Msg(exc.Message)  ICON(*Stop)
		EndTry		

	EndConstructor
	
