	BegSr txtVendor_Validating Access(*Private) Event(*this.txtVendor.Validating)
		DclSrParm sender *Object
		DclSrParm e System.ComponentModel.CancelEventArgs
		
		DclFld Vendor             Type(System.Int32)
		DclFld ImportDomesticFlag Type(String)
		
		ErrPro.SetControlError(sender *As Control,String.Empty)
		
		Try
			If Not String.IsNullOrEmpty(txtVendor.Text)
				Try
					Vendor = Convert.ToInt32(txtVendor.Text)
				Catch Name(Err) Type(System.Exception)
					ErrPro.SetControlError(sender *As Control,'Invalid number')
					lblVendorDesc.Text = String.Empty
					LeaveSR
				EndTry
							
				If not ItemsBO.GetVendor(Vendor)
					ErrPro.SetControlError(sender *As Control,'Vendor is not defined or you are not authorised to it')
					lblVendorDesc.Text = String.Empty
					txtVendor.Select()
				Else
					lblVendorDesc.Text = ItemsBO.VendorName
					
						ImportDomesticFlag =  ItemsBO.GetImportDoemstic(Vendor)
						If ImportDomesticFlag = 'Y'
							rbImport.Checked = *True
						Else
							rbDomestic.Checked = *True
						Endif					
				EndIf
			Else
				ErrPro.SetControlError(sender *As Control,'Please enter a Vendor')
				lblVendorDesc.Text = String.Empty
			EndIf
		Catch Name(exc) Type(Exception)
			//MsgBox TITLE('Error') Msg(exc.Message)  ICON(*Stop)
		EndTry
	
	EndSr
