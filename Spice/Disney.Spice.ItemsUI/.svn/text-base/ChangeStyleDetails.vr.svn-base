	BegSr btnNext_Click Access(*Private) Event(*this.btnNext.Click)
		DclSrParm sender *Object
		DclSrParm e System.EventArgs
		
		DclFld Index Type(Int16)
		DclFld PendingString Type(String)
		
		*this.ValidateChildren()
		
		Try
			If ErrPro.ErrorCount = *Zero
				If Not String.IsNullOrEmpty(lblPendSubClass.Text)
					PendingString = lblPendSubClass.Text.Remove(0,1)
					Index = PendingString.IndexOf(')')
					PendingString = PendingString.Remove(Index)
					pItemStyle.PendingSubClass = PendingString.Trim()
				EndIf
				
				ItemsBO.LongDescription    = txtLongDesc.Text
				ItemStyle.LongDescription  = txtLongDesc.Text
				ItemsBO.LocalDescription   = txtLocalDescription.Text
				ItemStyle.LocalDescription = txtLocalDescription.Text
				m_ItemSizeOverride.Description = txtLongDesc.Text
				m_ItemSizeOverride.LocalDescription = txtLocalDescription.Text
				ItemStyle.ShortDescription = txtShortDesc.Text
				ItemStyle.VendorStyle      = txtVendorStyle.Text
							
				ItemStyle.LicenceCode      = cboLicence.SelectedValue.ToString()
				
				ItemStyle.CompositionCode  = txtCompoCode.Text
				ItemStyle.CommodityCode    = txtCommodity.Text
				ItemStyle.CountryCode      = txtOrigin.Text
				ItemStyle.CharacterCode    = txtChar.Text
				
				If Not String.IsNullOrEmpty(lblPendChar.Text)
					PendingString = lblPendChar.Text.Remove(0,1)
					PendingString = PendingString.Replace(')',String.Empty)
					pItemStyle.PendingCharCode  = PendingString
				EndIf
				
				If (txtSeason.Text <> *Nothing)
					ItemStyle.SeasonCode    = txtSeason.Text
				EndIf
							
				If (cboTicketType.SelectedValue <> *Nothing)
					ItemStyle.TicketType = cboTicketType.SelectedValue.ToString()
				EndIf
				
				If(cboAgeGroup.SelectedValue <> *Nothing)
					pItemStyle.AgeGroup      = cboAgeGroup.SelectedValue.ToString()
				EndIf
				
				If(txtPromo.Text <> *Nothing)
					pItemStyle.PromotionCode = txtPromo.Text
				EndIf
							
				If Not String.IsNullOrEmpty(lblPendPromo.Text)
					PendingString = lblPendPromo.Text.Remove(0,1)
					PendingString = PendingString.Replace(')',String.Empty)
					ItemStyle.PendingPromoCode = PendingString
				EndIf
				
				Select
				When rbGenderUnisex.Checked
					ItemStyle.GenderCode = 'U'
				When rbGenderMale.Checked
					ItemStyle.GenderCode = 'M'
				Other
					ItemStyle.GenderCode = 'F'
				EndSL
				
				If rbOnlineNo.Checked
					ItemStyle.OnlineInd = 'N'
				Else
					ItemStyle.OnlineInd = 'Y'
				EndIf
				
				If rbLocationNo.Checked
					ItemStyle.LocationInd = 'N'
				Else
					ItemStyle.LocationInd = 'Y'
				EndIf
				
				If rbImport.Checked
					ItemStyle.ImpDomInd = 'I'
				Else
					ItemStyle.ImpDomInd = 'D'
				EndIf
				
				If (cboSellingChannel.SelectedIndex = -1)
					errorProvider1.SetError(cboSellingChannel, 'Select a Selling Channel')
					LeaveSR
				Else
					If (cboSellingChannel.SelectedValue <> *Nothing)
						ItemsBO.SellingChannel = cboSellingChannel.SelectedValue.ToString()
						ItemStyle.SellingChannel = cboSellingChannel.SelectedValue.ToString()
					EndIf				
				EndIf
				
				*this.DialogResult = DialogResult.OK
			EndIf
		Catch Name(exc) Type(Exception)
			//MsgBox TITLE('Error') Msg(exc.Message)  ICON(*Stop)
		EndTry	
	EndSr
	
/EndRegion
	
	BegSr cboSellingChannel_SelectedIndexChanged Access(*Private) Event(*this.cboSellingChannel.SelectedIndexChanged)
		DclSrParm sender *Object
		DclSrParm e System.EventArgs

		DclFld retCode Type(*Integer4)
		DclFld sb Type(StringBuilder)
		
		sb = *New StringBuilder() 
		
		Try
			If (m_formLoaded = *True)			
				sb.Append("Do you want to set this attribute for all sizes?")
				MSGBOX MSG (sb.ToString()) TITLE ('Update Item Sizes - Selling Channel') BUTTONS (*YesNo) ICON (*Exclamation) DFTBUTTON (2) TOPMOST (*YES) RTNCODE(retCode) 
				If (RetCode = 6) 
					//MSGBOX MSG ('Yes pressed')
					m_ItemSizeOverride.OverrideSellingChannel = *True
					m_ItemSizeOverride.SellingChannel = cboSellingChannel.SelectedValue.ToString()
					ItemStyle.SellingChannel = cboSellingChannel.SelectedValue.ToString()					
				EndIf 
				If (RetCode = 2) 
					//MSGBOX MSG ('Cancel pressed')
					m_ItemSizeOverride.OverrideSellingChannel = *False
					ItemStyle.SellingChannel = ""
				EndIf 
				If (RetCode = 7) 
					//MSGBOX MSG ('No pressed')
					m_ItemSizeOverride.OverrideSellingChannel = *False
					ItemStyle.SellingChannel = ""
				EndIf 
			EndIf
		Catch Name(exc) Type(Exception)
			//MsgBox TITLE('Error') Msg(exc.Message)  ICON(*Stop)
		EndTry	
	EndSr
	
