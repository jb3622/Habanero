	BegSr LoadEvent Access(*Private) Event(*this.Load)
		DclSrParm sender *Object
		DclSrParm e System.EventArgs
		
		m_formLoaded = *False
		
        ErrPro = *New Disney.Spice.ErrorProvider.ErrorProviderWithCount(*this.components)
		ErrPro.BlinkStyle = System.Windows.Forms.ErrorBlinkStyle.NeverBlink
		ErrPro.ContainerControl = *this
		
		// Determine the Continent - In Europe the Composition and Commodity codes can be changed.
		// If Environment.Domain          = 'EMEA'
		If (DataCache.IsCommodityCodeEditable = *True)
			btnCommodityLookUp.Enabled = *True
			txtCommodity.Enabled       = *True
		Else
			btnCommodityLookUp.Enabled = *False
			txtCommodity.Enabled       = *False
		EndIf
		
		If (DataCache.IsCompositionCodeEditable  = *True)
			btnCompoCodelookUp.Enabled = *True			 
			txtCompoCode.Enabled       = *True
		Else
			txtCompoCode.Enabled       = *False
			btnCompoCodelookUp.Enabled = *False			
		EndIf
		
		m_formLoaded = *True
	EndSr

	BegSr FormClosingEvent Access(*Private) Event(*this.FormClosing)
		DclSrParm sender Type(*Object)
		DclSrParm e Type(System.Windows.Forms.FormClosingEventArgs)
        
		DclFld ReturnCode Type(Int16)
		
		If *this.DialogResult = DialogResult.Cancel
			MsgBox 'Are you sure you want to Cancel?' RtnCode(ReturnCode) Buttons(*YesNo) Title("Cancel ?") Icon(*Question)
			If ReturnCode.Equals(6)
				*this.DialogResult = DialogResult.Abort
			Else
				e.Cancel = *true
			EndIf
		EndIf
	EndSr
	
	BegSr Change Access(*Public)
		
		DclFld dt Type(DataTable)
		
		//Clear Errors on back button
		If DialogResult = DialogResult.Retry
			ErrPro.ClearAll()
		Endif
		
		Try 
		
			// Retrieve Item details and populate textboxes, etc
			lblMarkets.Text = ItemsBO.SelectedMarkets
			
			txtClass.Text  = ItemStyle.Class.ToString('0000')
			ItemsBO.GetClass(pItemStyle.Class)
			lblClassDesc.Text = ItemsBO.ClassName
			
			txtVendor.Text = ItemStyle.Vendor.ToString('00000')
			ItemsBO.GetVendor(pItemStyle.Vendor)
			lblVendorName.Text = ItemsBO.VendorName
			
			txtStyle.Text  = ItemStyle.Style.ToString('0000')
			
			txtColour.Text = ItemStyle.Colour.ToString('000')
			ItemsBO.GetColour(ItemStyle.Colour)
			lblColourDesc.Text = ItemsBO.ColourName
			
			txtSubClass.Text = ItemStyle.SubClass
			ItemsBO.GetSubClass(ItemStyle.SubClass,pItemStyle.Class)
			lblSubClassDesc.Text = ItemsBO.SubClassName
			If pItemStyle.PendingSubClass <> String.Empty
				lblPendSubClass.Text = "(" + ItemStyle.PendingSubClass + ")"
			Else
				lblPendSubClass.Text = String.Empty
			EndIf
			
			txtLongDesc.Text    = ItemStyle.LongDescription
			txtShortDesc.Text   = ItemStyle.ShortDescription
			txtLocalDescription.Text = ItemStyle.LocalDescription
			txtVendorStyle.Text = ItemStyle.VendorStyle
							
			// Licence code
			ItemsBO.GetLicenceTbl()
			cboLicence.DataSource = ItemsBO.SpiceDataSet.Tables("FilteredLicence")
			cboLicence.DisplayMember = "CodeDescription"
			cboLicence.ValueMember   = "Code"
			If pItemStyle.LicenceCode <> String.Empty
				cboLicence.SelectedValue = ItemStyle.LicenceCode
			EndIf
					
			// Composition Code
			txtCompoCode.Text = ItemStyle.CompositionCode
			ItemsBO.GetCompositionCode(ItemStyle.CompositionCode)
			lblCompoCodeDesc.Text = ItemsBO.CompositionDescription
			
			// Commodity Code
			txtCommodity.Text = ItemStyle.CommodityCode
			ItemsBO.GetCommodityCode(ItemStyle.CommodityCode)
			lblCommodityDesc.Text = ItemsBO.CommodityDescription
			
			// Country of Origin
			txtOrigin.Text = ItemStyle.CountryCode
			ItemsBO.GetCountry(ItemStyle.CountryCode)
			lblOrigin.Text = ItemsBO.CountryName
			
			// Character code
			txtChar.Text = ItemStyle.CharacterCode
			ItemsBO.GetChar(ItemStyle.CharacterCode)
			lblCharName.Text = ItemsBO.CharacterName
			If pItemStyle.PendingCharCode <> String.Empty
				lblPendChar.Text = "(" + ItemStyle.PendingCharCode + ")"
			Else
				lblPendChar.Text = String.Empty
			EndIf
			
			// Season Code
			txtSeason.Text = ItemStyle.SeasonCode
			ItemsBO.GetSeason(ItemStyle.SeasonCode)
			lblSeason.Text = ItemsBO.SeasonDesc
			
			// Ticket Type
			ItemsBO.GetTicketTypeTbl()
			cboTicketType.DataSource = ItemsBO.SpiceDataSet.Tables("TicketType")
			cboTicketType.DisplayMember = "CodeDescription"
			cboTicketType.ValueMember   = "TicketTypeCode"
			If pItemStyle.TicketType <> String.Empty
				cboTicketType.SelectedValue = ItemStyle.TicketType
			EndIf
			
			// Selling Channel
			ItemsBO.GetSellingChannelTbl()
			cboSellingChannel.DataSource = ItemsBO.SpiceDataSet.Tables("SellingChannelTable")
			cboSellingChannel.DisplayMember = "CodeAndDescription" 
			cboSellingChannel.ValueMember   = "SellingChannelCode"
			If (ItemsBO.SellingChannel = *Nothing *Or ItemsBO.SellingChannel = String.Empty)
				cboSellingChannel.SelectedIndex = -1
				dt = cboSellingChannel.DataSource *AS DataTable			
				DclFld x *Integer2
				For Index(x = 0) to(dt.Rows.Count - 1)
					If (dt.Rows[x]["ItemsDefault"].ToString() = "Y")
						cboSellingChannel.SelectedValue = dt.Rows[x]["SellingChannelCode"]
					EndIf
				EndFor				
			Else
				cboSellingChannel.SelectedValue = ItemsBO.SellingChannel //ItemStyle.SellingChannel
			EndIf
			
			// Target Age Group
			ItemsBO.GetAgeGroupTbl()
			cboAgeGroup.DataSource = ItemsBO.SpiceDataSet.Tables("AgeGroups")
			cboAgeGroup.DisplayMember = "GroupDescription"
			cboAgeGroup.ValueMember   = "AgeGroupID"
			If pItemStyle.AgeGroup <> String.Empty
				cboAgeGroup.SelectedValue = ItemStyle.AgeGroup
			EndIf
			
			// Promo Code
			txtPromo.Text = pItemStyle.PromotionCode
			ItemsBO.GetPromotion(pItemStyle.PromotionCode)
			lblPromoDesc.Text = ItemsBO.PromotionDescription
			If pItemStyle.PendingPromoCode <> String.Empty
				lblPendPromo.Text = "(" + ItemStyle.PendingPromoCode + ")"
			Else
				lblPendPromo.Text = String.Empty
			EndIf
			
			// Radio buttons
			If pItemStyle.ImpDomInd.Equals('I')
				rbImport.Checked = *true
			Else
				rbDomestic.Checked = *true
			EndIf
			
			Select
			When ItemStyle.GenderCode.Equals('U')
				rbGenderUnisex.Checked = *true
			When ItemStyle.GenderCode.Equals('M')
				rbGenderMale.Checked = *true
			Other
				rbGenderFemale.Checked = *true
			EndSL
			
			If ItemStyle.LocationInd.Equals('N')
				rbLocationNo.Checked = *true
			Else
				rbLocationYes.Checked = *true
			EndIf
			
			If ItemStyle.OnlineInd.Equals("N")
				rbOnlineNo.Checked = *true
			Else
				rbOnlineYes.Checked = *true
			EndIf
			
			// Audit fields
			txtCreateDateTime.Text = ItemStyle.CreateDate.ToString("dd-MM-yyyy-HH.mm.ss")
			txtCreateUser.Text     = ItemStyle.CreateUser
			
			If pItemStyle.ChangeDate <> DateTime.MinValue
				txtChangeDateTime.Text = ItemStyle.ChangeDate.ToString("dd-MM-yyyy-HH.mm.ss")
			Else
				txtChangeDateTime.Text = String.Empty
			EndIf
			
			txtChangedUser.Text = ItemStyle.ChangeUser
		
		Catch Name(exc) Type(Exception)
			//MsgBox TITLE('Error') Msg(exc.Message) ICON(*Stop)
		EndTry
		
		*this.ShowDialog()
		*this.Hide()
	EndSr
	/EndRegion
	
	/Region Description fields
	BegSr txtLongDesc_Validating Access(*Private) Event(*this.txtLongDesc.Validating)
		DclSrParm sender *Object
		DclSrParm e System.ComponentModel.CancelEventArgs
		
		ErrPro.SetControlError(txtLongDesc,String.Empty)
		If (txtLongDesc.Text = String.Empty)
			ErrPro.SetControlError(txtLongDesc,"Description must not be blank")
		EndIf
		
		Try
			ReturnText = ItemsBO.ReplaceInvalidChrs(txtLongDesc.Text.ToString())
			
			If (ReturnText <> txtLongDesc.Text)
				txtLongDesc.Text = ReturnText
				MsgBox Msg('All invalid characters have been replaced with spaces')
				txtLongDesc.Select()
			Endif	
		Catch Name(exc) Type(Exception)
			//MsgBox TITLE('Error') Msg(exc.Message)  ICON(*Stop)
		EndTry	
		
		DclFld retCode Type(*Integer4)
		DclFld sb Type(StringBuilder)
		
		sb = *New StringBuilder() 
			
		Try
			If (ItemForOnlineEcom() = *False)
				m_ItemSizeOverride.OverrideDescription = *True	
			Else
				If (m_formLoaded = *True *AND m_LongDescChanged = *True)			
					sb.Append("Do you want to set this attribute for all sizes?")
					MSGBOX MSG (sb.ToString()) TITLE ('Update Item Sizes - Description') BUTTONS (*YesNo) ICON (*Exclamation) DFTBUTTON (2) TOPMOST (*YES) RTNCODE(retCode) 
					m_ItemSizeOverride.Description = txtLongDesc.Text
					m_ItemSizeOverride.LocalDescription = txtLocalDescription.Text
					If (RetCode = 6) 
						//MSGBOX MSG ('Yes pressed')
						m_ItemSizeOverride.OverrideDescription = *True					
					EndIf 
					If (RetCode = 2) 
						//MSGBOX MSG ('Cancel pressed')
						m_ItemSizeOverride.OverrideDescription = *False
					EndIf 
					If (RetCode = 7) 
						//MSGBOX MSG ('No pressed')
						m_ItemSizeOverride.OverrideDescription = *False
					EndIf
					m_LongDescChanged = *False
				EndIf
			EndIf
		Catch Name(exc) Type(Exception)
			//MsgBox TITLE('Error') Msg(exc.Message)  ICON(*Stop)
		EndTry					
	EndSr
	
	BegFunc ItemForOnlineEcom Type(Boolean) Access(*Private) 		
		DclFld result Type(Boolean) Inz(*False)		
		DclFld dataRow Type(DataRow)
		DclFld forOnlineEcom Type(String)
		
		Try
			dataRow = ItemsBO.SpiceDataSet.Tables["SellingChannelTable"].Rows.Find(cboSellingChannel.SelectedValue.ToString())
			If (dataRow <> *Nothing)		
				forOnlineEcom = dataRow["OnlineECOM"] *AS String				
				If (forOnlineEcom <> *Nothing)
					If (forOnlineEcom.Trim() = "Y")
						result = *True
					Else
						result = *False
					EndIf
				EndIf
			EndIf
		Catch Name(exc) Type(Exception)
			//MsgBox TITLE('Error') Msg(exc.Message) ICON(*Stop)
		EndTry		
		LeaveSR result
	EndFunc
	
