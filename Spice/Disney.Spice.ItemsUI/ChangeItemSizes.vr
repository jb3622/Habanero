	BegSr btnNext_Click Access(*Private) Event(*this.btnNext.Click)
		DclSrParm sender *Object
		DclSrParm e System.EventArgs
				
		DclFld dv			Type(DataView)
		DclFld DuplicateChk Type(Boolean) Inz(*False)
		DclFld ErrorCheck	Type(Boolean) Inz(*False)
		DclFld sellingChannelItemSize Type(String)
		DclFld ecomSellingChannel Type(String)
		DclFld ecomSellingChannelExists Type(Boolean) Inz(*False)
		DclFld upcString    Type(String) Inz(String.Empty)
		DclFld duplicateUPCs   Type(ArrayList)
		DclFld overrideUPCs    Type(ArrayList)
		
		duplicateUPCs = *New ArrayList() // list of UPC's duplicated in the grid
		overrideUPCs = *New ArrayList() // UPC's added to file DS406JW
		
		Try	
			UpdateProperties()
			
			ErrPro.SetControlError(btnNext,String.Empty)
			*this.ValidateChildren()
							
			If ItemsBO.SpiceDataSet.Tables["ItemSizes"].Rows.Count > 1
				dv  = *New DataView(ItemsBO.SpiceDataSet.Tables("ItemSizes"))
				
				// Two lists - duplicate UPC's and list of UPC's kept in
				// DS406JW. Populate the lists
				ForEach SizeRow Collection(dv) Type(DataRowView)
					If Convert.ToInt64(SizeRow.Item("VendorUPC")) <> *Zero
						upcString = SafeStringValue(SizeRow["VendorUPC"])	
						If ItemsBO.OverrideUPCCDValidation(upcString) = *true
							overrideUPCs.Add(upcString)
						Else
							If Not duplicateUPCs.Contains(upcString)
								duplicateUPCs.Add(upcString)
								DuplicateChk = *True
							Endif
						EndIf
					Endif
				EndFor	
				
				// Duplicate UPCs' found - invalid!
				If DuplicateChk = *True
					ErrPro.SetControlError(btnNext,"There are duplicate UPC's. Please Correct")					
					LeaveSR
				EndIf
								
				// Further validation of size rows
				ForEach dgvRow Collection(dgvSizes.Rows) Type(DataGridViewRow)
					dgvRow.Cells["OED"].Value = *this.DateTimeToStringDateValue(SafeDateValue(dgvRow.Cells["OfferEffectiveDate"].Value))
					
					// selling channel
					sellingChannelItemSize = dgvRow.Cells["SellingChannel"].Value.ToString()
					ItemsBO.SellingChannel = sellingChannelItemSize
					If (ItemForOnlineEcom(sellingChannelItemSize) = *True)
						ecomSellingChannelExists = *True
						ecomSellingChannel = sellingChannelItemSize										
					EndIf
									    			
					If Convert.ToInt64(dgvRow.Cells["VendorUPC"].Value) <> *Zero					
						ResultUPC = SafeInt64Value(dgvRow.Cells["VendorUPC"].Value)
						
						// Validate the UPC check digit identify duplicates 
						// (unless the UPC is an override UPC - kept in file DS406JW)
						upcString = SafeStringValue(dgvRow.Cells["VendorUPC"].Value)
						If Not overrideUPCs.Contains(upcString)
						    // Calculate UPC Checkdigit
							UPCcheckDigit =  ItemsBO.ChkUPCcheckDigit(ResultUPC)
							If	UPCcheckDigit = *False
								dgvRow.Cells["VendorUPC"].Style.BackColor = Color.Red
								ErrorCheck = *True
							EndIf	
							
							If Not ItemsBO.ChkUPCunique(ResultUPC, LongItemNumber)
								dgvRow.Cells["VendorUPC"].Style.BackColor = Color.Red
								ErrorCheck = *True
							EndIf
						Endif
						
						GridClass    = SafeInt16Value(dgvRow.Cells["Class"].Value)
						GridVendor   = SafeInt16Value(dgvRow.Cells["Vendor"].Value)
						GridStyle    = SafeInt16Value(dgvRow.Cells["Style"].Value)
						GridColour   = SafeInt16Value(dgvRow.Cells["Colour"].Value)
						GridSize     = SafeInt16Value(dgvRow.Cells["Size"].Value)
						
						LongItemNumber = *New Disney.DA.IP400.LongItemNumber()
						longItemNumber.Class  = GridClass
						LongItemNumber.Vendor = GridVendor
						LongItemNumber.Style  = GridStyle
						LongItemNumber.Colour = GridColour
						LongItemNumber.Size   = GridSize
						
					Endif
				EndFor
		
				If ErrorCheck = *True
					ErrPro.SetControlError(btnNext,"There are errors. Please Correct")
					LeaveSR
				EndIf
			EndIf
			
			If (ecomSellingChannelExists = *True)				
				ItemsBO.SellingChannel = ecomSellingChannel
			EndIf
			
			If ErrPro.ErrorCount = *Zero
				*this.DialogResult = DialogResult.OK
			EndIf
		Catch Name(err) Type(System.Exception)	
			Throw err
		EndTry
			
	EndSr
	
