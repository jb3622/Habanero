			DclFld GridPackLength      Type(Decimal)
			DclFld GridPackWidth       Type(Decimal)
							
			If (dgvSizes["PackLocked",e.RowIndex].Value.ToString()) = "Y"
				MsgBox "These Pack/Carton details have been updated by the DC and cannot be changed" Icon(*Exclamation) Title("Pack details locked")
				LeaveSR
			EndIf
			
			GridClass    = SafeInt16Value(dgvSizes.Item["Class",    e.RowIndex].Value)
			GridVendor   = SafeInt32Value(dgvSizes.Item["Vendor",   e.RowIndex].Value)
			GridStyle    = SafeInt16Value(dgvSizes.Item["Style",    e.RowIndex].Value)
			GridColour   = SafeInt16Value(dgvSizes.Item["Colour",   e.RowIndex].Value)
			GridSize     = SafeInt16Value(dgvSizes.Item["Size",     e.RowIndex].Value)
			GridSizeDesc = SafeStringValue(dgvSizes.Item["SizeDesc", e.RowIndex].Value)
				
			Pending     = SafeStringValue(dgvSizes.Item["PendingFlag", e.RowIndex].Value)
			ChgPackDtls = *New ChangeItemPackDetails(PgmDB, Environment, Pending, GridSize, GridSizeDesc) 
					
			If Pending  = 'Y' 
				ItemsBO.GetPendingCasePackDetails(GridClass, GridVendor, GridStyle, GridColour, GridSize)
				
				If ItemsBO.PendingCasePackType <> String.Empty
					ChgPackDtls.CasePackType = ItemsBO.PendingCasePackType
				Else
					ChgPackDtls.CasePackType = dgvSizes["CasePackType",e.RowIndex].Value.ToString()
				Endif
				
				If ItemsBO.PendingCasePackQty <> 0
					ChgPackDtls.PackQty      = ItemsBO.PendingCasePackQty
				Else
					ChgPackDtls.PackQty      = SafeInt32Value(dgvSizes.Item["CaseQuantity",e.RowIndex].Value)
				Endif
				
				If ItemsBO.PendingDistroInnerQty <> 0
					ChgPackDtls.DistroQty    = ItemsBO.PendingDistroInnerQty
				Else
					ChgPackDtls.DistroQty    = SafeInt32Value(dgvSizes.Item["DistroQuantity",e.RowIndex].Value)
				Endif
				
				If ItemsBO.PendingWeight <> 0 
					ChgPackDtls.PackWeight   = ItemsBO.PendingWeight
				Else
					ChgPackDtls.PackWeight   = SafeDecimalValue(dgvSizes.Item["Weight",e.RowIndex].Value)
				Endif
				
				If ItemsBO.PendingHeight <> 0
					ChgPackDtls.PackHeight   = ItemsBO.PendingHeight
				Else
					ChgPackDtls.PackHeight   = SafeDecimalValue(dgvSizes.Item["Height",e.RowIndex].Value)
				Endif
				
				If ItemsBO.PendingLength <> 0
					ChgPackDtls.PackLength   = ItemsBO.PendingLength
				Else
					ChgPackDtls.PackLength   = SafeDecimalValue(dgvSizes.Item["Length",e.RowIndex].Value)
				Endif
				
				If  ItemsBO.PendingWidth <> 0	
					ChgPackDtls.PackWidth    = ItemsBO.PendingWidth 
				Else
					ChgPackDtls.PackWidth    = SafeDecimalValue(dgvSizes.Item["Width",e.RowIndex].Value)
				Endif
				
				ChgPackDtls.ShowDialog()
				LeaveSr
			Endif
			
			If dgvSizes["PendingCasePackType",e.RowIndex].Value.ToString() <> String.Empty 
				ChgPackDtls.CasePackType = dgvSizes["PendingCasePackType",e.RowIndex].Value.ToString()
			Else
				ChgPackDtls.CasePackType = dgvSizes["CasePackType",e.RowIndex].Value.ToString()
			Endif
			
			If dgvSizes["PendingPackQty",e.RowIndex].Value.ToString() <> String.Empty And + 
			   dgvSizes["PendingPackQty",e.RowIndex].Value.ToString() <> '0'
				ChgPackDtls.PackQty      = SafeInt32Value(dgvSizes.Item["PendingPackQty",e.RowIndex].Value)
			Else
				ChgPackDtls.PackQty      = SafeInt32Value(dgvSizes.Item["CaseQuantity",e.RowIndex].Value)
			Endif
			
			If dgvSizes["PendingDistroQty",e.RowIndex].Value.ToString() <> String.Empty And +
				dgvSizes["PendingDistroQty",e.RowIndex].Value.ToString() <> '0'
				ChgPackDtls.DistroQty    = SafeInt32Value(dgvSizes.Item["PendingDistroQty",e.RowIndex].Value)
			Else
				ChgPackDtls.DistroQty    = SafeInt32Value(dgvSizes.Item["DistroQuantity",e.RowIndex].Value)
			Endif
			
			If dgvSizes["PendingPackWeight",e.RowIndex].Value.ToString() <> String.Empty And +
				dgvSizes["PendingPackWeight",e.RowIndex].Value.ToString() <> '0'
				ChgPackDtls.PackWeight   = SafeDecimalValue(dgvSizes.Item["PendingPackWeight",e.RowIndex].Value)
			Else
				ChgPackDtls.PackWeight   = SafeDecimalValue(dgvSizes.Item["Weight",e.RowIndex].Value )
			Endif
					
			If dgvSizes["PendingPackHeight",e.RowIndex].Value.ToString() <> String.Empty And +
				dgvSizes["PendingPackHeight",e.RowIndex].Value.ToString() <> '0'
				ChgPackDtls.PackHeight   = SafeDecimalValue(dgvSizes.Item["PendingPackHeight",e.RowIndex].Value )
			Else
				ChgPackDtls.PackHeight   = SafeDecimalValue(dgvSizes.Item["Height",e.RowIndex].Value )
			Endif
			
			If dgvSizes["PendingPackLength",e.RowIndex].Value.ToString() <> String.Empty And +
				dgvSizes["PendingPackLength",e.RowIndex].Value.ToString() <> '0'
				ChgPackDtls.PackLength   = SafeDecimalValue(dgvSizes.Item["PendingPackLength",e.RowIndex].Value )
			Else
				ChgPackDtls.PackLength   = SafeDecimalValue(dgvSizes.Item["Length",e.RowIndex].Value)
			Endif
			
			If dgvSizes["PendingPackWidth",e.RowIndex].Value.ToString() <> String.Empty And +
				dgvSizes["PendingPackWidth",e.RowIndex].Value.ToString() <> '0'
				ChgPackDtls.PackWidth    = SafeDecimalValue(dgvSizes.Item["PendingPackWidth",e.RowIndex].Value )
			Else
				ChgPackDtls.PackWidth    = SafeDecimalValue(dgvSizes.Item["Width",e.RowIndex].Value  )
			Endif
									
			ChgPackDtls.ShowDialog()
					
			//Only update if anything has changed
			If ChgPackDtls.DialogResult = DialogResult.OK
				
				GridPackQty    = SafeInt32Value(dgvSizes.Item["CaseQuantity",e.RowIndex].Value)
				GridDistroQty  = SafeInt32Value(dgvSizes.Item["DistroQuantity",e.RowIndex].Value)
				GridPackWeight = SafeDecimalValue(dgvSizes.Item["Weight",e.RowIndex].Value)
				GridPackHeight = SafeDecimalValue(dgvSizes.Item["Height",e.RowIndex].Value)
				GridPackLength = SafeDecimalValue(dgvSizes.Item["Length",e.RowIndex].Value)
				GridPackWidth  = SafeDecimalValue(dgvSizes.Item["Width",e.RowIndex].Value)
			 
				If ChgPackDtls.CasePackType <> dgvSizes["CasePackType",e.RowIndex].Value.ToString() Or +
					ChgPackDtls.PackWeight  <> GridPackWeight                                       Or +
					ChgPackDtls.PackHeight  <> GridPackHeight                                       Or +
					ChgPackDtls.PackLength  <> GridPackLength                                       Or +
					ChgPackDtls.PackWidth   <> GridPackWidth 
					
					dgvSizes.Item["PendingCasePackType",e.RowIndex].Value = ChgPackDtls.CasePackType
					dgvSizes.Item["PendingPackWeight",e.RowIndex].Value = ChgPackDtls.PackWeight
					dgvSizes.Item["PendingPackHeight",e.RowIndex].Value = ChgPackDtls.PackHeight
					dgvSizes.Item["PendingPackLength",e.RowIndex].Value = ChgPackDtls.PackLength
					dgvSizes.Item["PendingPackWidth",e.RowIndex].Value = ChgPackDtls.PackWidth
					dgvSizes.Item["Pending",e.RowIndex].Value = "P"
				Else
					dgvSizes.Item["PendingCasePackType",e.RowIndex].Value = String.Empty
					dgvSizes.Item["PendingPackWeight",e.RowIndex].Value = 0
					dgvSizes.Item["PendingPackHeight",e.RowIndex].Value = 0
					dgvSizes.Item["PendingPackLength",e.RowIndex].Value = 0
					dgvSizes.Item["PendingPackWidth",e.RowIndex].Value = 0
				Endif
				
				If ChgPackDtls.PackQty <> GridpackQty Or ChgPackDtls.DistroQty <> GridDistroQty
					dgvSizes.Item["PendingPackQty",e.RowIndex].Value = ChgPackDtls.PackQty
					dgvSizes.Item["PendingDistroQty",e.RowIndex].Value = ChgPackDtls.DistroQty
					dgvSizes.Item["Pending",e.RowIndex].Value = "P"
				Else
					dgvSizes.Item["PendingPackQty",e.RowIndex].Value = 0
					dgvSizes.Item["PendingDistroQty",e.RowIndex].Value = 0
				Endif
			EndIf
		Catch Name(ex) Type(System.Exception)	
			Throw ex
		EndTry
	EndSr
	
	BegSr dgvSizes_CellValidated Access(*Private) Event(*this.dgvSizes.CellValidated)
		DclSrParm sender *Object
		DclSrParm e System.Windows.Forms.DataGridViewCellEventArgs
		
		DclFld str            Type(String)
		
		If dgvSizes.Columns(e.ColumnIndex).Name.Equals("LongDescription") 
		
			str = dgvSizes[e.ColumnIndex,e.RowIndex].Value.ToString()
			str = str.ToUpper()
			str = ItemsBO.ReplaceInvalidChrs(str)
			dgvSizes[e.ColumnIndex,e.RowIndex].Value = str
		EndIf	
		 
		If dgvSizes.Columns(e.ColumnIndex).Name.Equals("SellingChannel") 
			str	= dgvSizes["SellingChannel",e.RowIndex].Value.ToString()	
			If(str.Trim() = "Z")
				dgvSizes["SOControl",e.RowIndex].Value = "1" //Sell Out Immediately
			EndIf
		EndIf	
				
	EndSr
	
	BegSr dgvSizes_CellValidating Access(*Private) Event(*this.dgvSizes.CellValidating)
		DclSrParm sender *Object
		DclSrParm e System.Windows.Forms.DataGridViewCellValidatingEventArgs
				
		DclFld GridClass	  Type(Int16)
		DclFld GridVendor	  Type(Int32)
		DclFld GridStyle	  Type(Int16)
		DclFld GridColour	  Type(Int16)
		DclFld GridSize		  Type(Int16)
		DclFld Market         Type(String)
		DclFld dv             Type(DataView)
				
		Try		
			dgvSizes[e.ColumnIndex,e.RowIndex].ErrorText = String.Empty
			btnNext.Enabled = *True
			
			If dgvSizes.Columns(e.ColumnIndex).Name.Equals("LongDescription") 
				If e.FormattedValue.ToString() = String.Empty				
					MsgBox TITLE('Description') Msg('The description can not be blank.') ICON(*Exclamation)
					dgvSizes.CancelEdit()
					LeaveSR
				EndIf
			EndIf
					
			If dgvSizes.Columns(e.ColumnIndex).Name.Equals("VendorUPC") 		
				 					
				dgvSizes.Item(e.ColumnIndex,e.RowIndex).ErrorText = String.Empty
				If Not String.IsNullOrEmpty(e.FormattedValue.ToString())
					If Int64.TryParse(e.FormattedValue.ToString(),*ByRef ResultUPC)
						If ResultUPC <> *Zero and ResultUPC	<> UPCSave
							GridClass  = SafeInt16Value(dgvSizes.Item["Class", e.RowIndex].Value)
							GridVendor = SafeInt32Value(dgvSizes.Item["Vendor",e.RowIndex].Value)
							GridStyle  = SafeInt16Value(dgvSizes.Item["Style", e.RowIndex].Value )
							GridColour = SafeInt16Value(dgvSizes.Item["Colour",e.RowIndex].Value )
							GridSize   = SafeInt16Value(dgvSizes.Item["Size",  e.RowIndex].Value )
															
							// Check if item has been Shipped or If there are outstanding PO's
							If ItemsBO.ChkItemShipped(GridClass,GridVendor,GridStyle,GridColour,GridSize)
								MsgBox "UPC cannot be changed : Item has shipped"
								dgvSizes.CancelEdit()
								LeaveSR
							EndIf
				
							If Not ItemsBO.ChkLineOrderStatus(GridClass,GridVendor,GridStyle,GridColour,GridSize)
								MsgBox "UPC cannot be changed : Orders are outstanding"
								dgvSizes.CancelEdit()
								LeaveSR
							EndIf
														
							// Calculate UPC Checkdigit
							UPCcheckDigit =  ItemsBO.ChkUPCcheckDigit(ResultUPC)
							If	UPCcheckDigit = *False
								dgvSizes.Item(e.ColumnIndex,e.RowIndex).ErrorText = "UPC CheckDigit not valid, Please correct."
								btnNext.Enabled = *False							
								LeaveSR
							EndIf
							
							LongItemNumber = *New Disney.DA.IP400.LongItemNumber()
							longItemNumber.Class  = GridClass
							LongItemNumber.Vendor = GridVendor
							LongItemNumber.Style  = GridStyle
							LongItemNumber.Colour = GridColour
							LongItemNumber.Size   = GridSize
							
							If Not ItemsBO.ChkUPCunique(ResultUPC, LongItemNumber)
								dgvSizes.Item(e.ColumnIndex,e.RowIndex).ErrorText = "UPC not unique, enter a different UPC"
								btnNext.Enabled = *False
								LeaveSR
							EndIf
									
							If Not ItemsBO.ChkVendorUPCunique(ResultUPC, LongItemNumber)
								dgvSizes.Item(e.ColumnIndex,e.RowIndex).ErrorText = "UPC not unique, enter a different UPC"
								btnNext.Enabled = *False
								LeaveSR
							EndIf
									
							If Not ItemsBO.ChkDisneyUPCunique(ResultUPC, LongItemNumber)
								dgvSizes.Item(e.ColumnIndex,e.RowIndex).ErrorText = "UPC not unique, enter a different UPC"
								btnNext.Enabled = *False
								LeaveSR
							EndIf
							
							dv = *New Dataview(ItemsBO.SpiceDataSet.Tables["AuthorisedMarkets"])
							dv.RowFilter = "Select = true"
							ForEach MarketRow Collection(dv) Type(DataRow)
								Market = SafeStringValue(MarketRow["IPMarket"])
								
								If Not ItemsBO.ChkVendorUPCunique(ResultUPC, LongItemNumber, Market)
									dgvSizes.Item(e.ColumnIndex,e.RowIndex).ErrorText = "UPC not unique, enter a different UPC"
									btnNext.Enabled = *False
									LeaveSR
								EndIf
							EndFor
						EndIf
					Else
						MsgBox Title('Item - Change Sizes - Error') MSG('Invalid number entered') Icon(*Stop)
						dgvSizes.CancelEdit()
					EndIf
				Else
					dgvSizes.CancelEdit()
				EndIf
			Else
				If dgvSizes.Item["VendorUPC",e.RowIndex].ErrorText <> String.Empty
					btnNext.Enabled = *False
				Endif
			Endif	
		Catch Name(err) Type(System.Exception)	
			Throw err
		EndTry
	EndSr
		
	BegSr dgvSizes_CellEnter Access(*Private) Event(*this.dgvSizes.CellEnter)
		DclSrParm sender *Object
		DclSrParm e System.Windows.Forms.DataGridViewCellEventArgs
		
		DclFld Remainder Type(Int16)
		
		Try
			Remainder = 0
			
			If dgvSizes.Columns(e.ColumnIndex).Name.Equals("VendorUPC") 
				UPCSave   = SafeInt64Value(dgvSizes.Item["VendorUPC",  e.RowIndex].Value)
				
				Remainder = %rem(e.RowIndex, 2)
				If Remainder > 0
					dgvSizes.Item["VendorUPC", e.RowIndex].Style.BackColor = Color.FromArgb(192, 255, 192)
				Else
					dgvSizes.Item["VendorUPC", e.RowIndex].Style.BackColor = Color.White
				Endif			
			Endif		
		Catch Name(exc) Type(System.Exception)	
			Throw exc
		EndTry		
	EndSr
	

