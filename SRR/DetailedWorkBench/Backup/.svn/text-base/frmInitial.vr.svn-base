	//--------------------------------------------------------------------------
	// Load List Box
	//--------------------------------------------------------------------------
    BegSr LoadListBox
       
        DclFld wIdx  	Type( *Integer4 )
       
//        Clear DeptArr
        uxDept.Items.Clear()
        wIdx = 0
		SetRange DSUSRDA FirstKey(AS400Profile)
		DoUntil DSUSRDA.IsEof
			Read DSUSRDA
			If Not DSUSRDA.IsEof
			  	DeptCode = USDEPT.ToString('000')
				Chain IPDEPTS Key(USDEPT)
				If IPDEPTS.IsFound
					DeptCode = %Trim(DeptCode + ' ' + DNAM)
				EndIf
				uxDept.Items.Add(DeptCode)
				IPUSer = USIPPR.trim()
//				DeptARR[wIdx] = DeptCode
				wIdx = wIdx + 1 
			EndIf
		EndDo	
    EndSr   
    
	//------------------------------------------------------------
	// Open database & files
	//------------------------------------------------------------
	BegSr OpenData 
	
		// Open DB connection.
		If Not AS400.IsOpen
			Connect AS400
		Endif
	
	
		// Open Department file
        If NOT IPDEPTS.IsOpen
			Open IPDEPTS
		Endif
	
	
		// Open Department file
        If NOT IPUSERS.IsOpen
			Open IPUSERS
		Endif	
		
	
		// Open Department file
        If NOT DSUSRDA.IsOpen
			Open DSUSRDA
		Endif
		
		
		// Country Code
        If NOT DS911AP.IsOpen
		   Open DS911AP
		   Country = *blanks
		   Read DS911AP
		   If Not DS911AP.IsEof
		      Country = DCOD
		   Endif   
		Endif		
				
	EndSr
	
